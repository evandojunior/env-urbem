    <sql>with x as &#xd;&#xa;&#x28;&#xd;&#xa;	select &#x28;sum&#x28;x.exec_pre_balancete&#x29;&#x3a;&#x3a;int&#x29;&#x3a;&#x3a;boolean as exec_pre_balancete from &#xd;&#xa;	&#x28;&#xd;&#xa;		-- SE ENCONTRAR NA TABELA N&#xc3;O DEVE EXECUTTAR&#x3a; -1&#x2b;1 &#x3d; 0 &#x3d;&#x3e; FALSE&#xd;&#xa;		select &#xd;&#xa;			-1 as exec_pre_balancete &#xd;&#xa;		from bi.plano_conta &#xd;&#xa;		where &#xd;&#xa;			exercicio &#x3d; &#x27;&#x24;&#x7b;EXERCICIO&#x7d;&#x27;&#xd;&#xa;		union&#xd;&#xa;	&#xd;&#xa;		select 1 as exec_pre_balancete &#xd;&#xa;	&#xd;&#xa;	&#x29; x&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;-- EXEC_RECARGA_PRE_BALANCETE &#x3d; Y, SER&#xc1; PROCESSADO O PRE BALANCETE&#xd;&#xa;-- EXEC_RECARGA_PRE_BALANCETE &#x3d; N, SER&#xc1; PROCESSADO O PRE BALANCETE  SOMENTE SE N&#xc3;O HAVER REGISTRO EM bi.plano_conta, CONFORME EM x&#xd;&#xa;select &#xd;&#xa;	case &#xd;&#xa;		when &#x27;Y&#x27; &#x3d; &#x27;&#x24;&#x7b;EXEC_RECARGA_PRE_BALANCETE&#x7d;&#x27;&#xd;&#xa;			then true&#xd;&#xa;		else x.exec_pre_balancete end&#xd;&#xa;    as exec_pre_balancete&#xd;&#xa;from x&#x3b;&#xd;&#xa;</sql>
