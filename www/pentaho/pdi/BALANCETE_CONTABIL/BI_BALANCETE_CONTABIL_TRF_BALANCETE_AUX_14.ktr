    <execute_each_row>Y</execute_each_row>
    <single_statement>N</single_statement>
    <replace_variables>Y</replace_variables>
    <quoteString>N</quoteString>
    <sql>INSERT INTO bi.balancete_14&#xd;&#xa;	&#x28;&#xd;&#xa;          cod_estrutural&#xd;&#xa;        , cod_plano&#xd;&#xa;        , num_orgao&#xd;&#xa;        , num_unidade&#xd;&#xa;        , cod_funcao&#xd;&#xa;        , cod_subfuncao&#xd;&#xa;        , num_programa&#xd;&#xa;        , num_acao&#xd;&#xa;        , cod_recurso&#xd;&#xa;        , natureza_despesa&#xd;&#xa;        , cod_empenho&#xd;&#xa;        , exercicio_empenho&#xd;&#xa;        , tipo_valor&#xd;&#xa;        , vl_lancamento&#xd;&#xa;        , cod_entidade&#xd;&#xa;        , cod_lote&#xd;&#xa;        , dt_lote&#xd;&#xa;        , exercicio&#xd;&#xa;        , mes&#xd;&#xa;        , tipo&#xd;&#xa;        , sequencia&#xd;&#xa;        , oid_temp&#xd;&#xa;        , cod_sistema&#xd;&#xa;        , escrituracao&#xd;&#xa;		, indicador_superavit&#xd;&#xa;   	    , natureza_saldo&#xd;&#xa;	&#x29;&#xd;&#xa;            SELECT pc.cod_estrutural&#xd;&#xa;                 , pc.cod_plano&#xd;&#xa;                 , rp_balancete.num_orgao&#xd;&#xa;                 , rp_balancete.num_unidade&#xd;&#xa;                 , rp_balancete.cod_funcao&#xd;&#xa;                 , rp_balancete.cod_subfuncao&#xd;&#xa;                 , rp_balancete.num_programa&#xd;&#xa;                 , rp_balancete.num_acao&#xd;&#xa;                 , rp_balancete.cod_recurso&#xd;&#xa;                 , rp_balancete.natureza_despesa&#xd;&#xa;                 , rp_balancete.cod_empenho&#xd;&#xa;                 , rp_balancete.exercicio_empenho&#xd;&#xa;                 , rp_balancete.tipo_valor&#xd;&#xa;                 , vl_lancamento_rp_processados AS vl_lancamento&#xd;&#xa;                 , rp_balancete.cod_entidade&#xd;&#xa;                 , rp_balancete.cod_lote&#xd;&#xa;                 , rp_balancete.dt_lote&#xd;&#xa;                 , rp_balancete.exercicio&#xd;&#xa;                 , EXTRACT&#x28;MONTH from date_trunc&#x28;&#x27;month&#x27;, rp_balancete.dt_lote&#x29;&#x29; as mes   &#xd;&#xa;                 , rp_balancete.tipo&#xd;&#xa;                 , rp_balancete.sequencia&#xd;&#xa;                 , rp_balancete.oid_temp&#xd;&#xa;                 , rp_balancete.cod_sistema&#xd;&#xa;                 , pc.escrituracao&#xd;&#xa;				 , pc.indicador_superavit&#xd;&#xa;				 , pc.natureza_saldo&#xd;&#xa;              FROM bi.balancete_contabil_14_restos_pagar AS rp_balancete&#xd;&#xa;        INNER JOIN bi.plano_conta pc&#xd;&#xa;        		on pc.exercicio &#x3d; rp_balancete.exercicio&#xd;&#xa;             WHERE pc.exercicio &#x3d; &#x27;&#x24;&#x7b;EXERCICIO&#x7d;&#x27;&#xd;&#xa;               AND pc.cod_estrutural SIMILAR TO &#x27;5.3.2.7.0.00.00.00.00.00&#x7c;6.3.2.7.0.00.00.00.00.00&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;			   &#xd;&#xa;			   &#xd;&#xa;INSERT INTO bi.balancete_14&#xd;&#xa;	&#x28;&#xd;&#xa;		  cod_estrutural&#xd;&#xa;		, cod_plano&#xd;&#xa;		, num_orgao&#xd;&#xa;		, num_unidade&#xd;&#xa;		, cod_funcao&#xd;&#xa;		, cod_subfuncao&#xd;&#xa;		, num_programa&#xd;&#xa;		, num_acao&#xd;&#xa;		, cod_recurso&#xd;&#xa;		, natureza_despesa&#xd;&#xa;		, cod_empenho&#xd;&#xa;		, exercicio_empenho&#xd;&#xa;		, tipo_valor&#xd;&#xa;		, vl_lancamento&#xd;&#xa;		, cod_entidade&#xd;&#xa;		, cod_lote&#xd;&#xa;		, dt_lote&#xd;&#xa;		, exercicio&#xd;&#xa;        , mes&#xd;&#xa;		, tipo&#xd;&#xa;		, sequencia&#xd;&#xa;		, oid_temp&#xd;&#xa;		, cod_sistema&#xd;&#xa;		, escrituracao&#xd;&#xa;        , indicador_superavit&#xd;&#xa;  	    , natureza_saldo&#xd;&#xa;	&#x29;&#xd;&#xa;            SELECT pc.cod_estrutural&#xd;&#xa;                 , pc.cod_plano&#xd;&#xa;                 , rp_balancete.num_orgao&#xd;&#xa;                 , rp_balancete.num_unidade&#xd;&#xa;                 , rp_balancete.cod_funcao&#xd;&#xa;                 , rp_balancete.cod_subfuncao&#xd;&#xa;                 , rp_balancete.num_programa&#xd;&#xa;                 , rp_balancete.num_acao&#xd;&#xa;                 , rp_balancete.cod_recurso&#xd;&#xa;                 , rp_balancete.natureza_despesa&#xd;&#xa;                 , rp_balancete.cod_empenho&#xd;&#xa;                 , rp_balancete.exercicio_empenho&#xd;&#xa;                 , &#x27;C&#x27;&#xd;&#xa;                 , &#x28;vl_lancamento_rp_nao_processados&#x2a;-1&#x29; AS vl_lancamento&#xd;&#xa;                 , rp_balancete.cod_entidade&#xd;&#xa;                 , rp_balancete.cod_lote&#xd;&#xa;                 , rp_balancete.dt_lote&#xd;&#xa;                 , rp_balancete.exercicio&#xd;&#xa;                 , EXTRACT&#x28;MONTH from date_trunc&#x28;&#x27;month&#x27;, rp_balancete.dt_lote&#x29;&#x29; as mes   &#xd;&#xa;                 , rp_balancete.tipo&#xd;&#xa;                 , rp_balancete.sequencia&#xd;&#xa;                 , rp_balancete.oid_temp&#xd;&#xa;                 , rp_balancete.cod_sistema&#xd;&#xa;                 , pc.escrituracao&#xd;&#xa;                 , pc.indicador_superavit&#xd;&#xa;				 , pc.natureza_saldo&#xd;&#xa;              FROM bi.balancete_contabil_14_restos_pagar AS rp_balancete&#xd;&#xa;        INNER JOIN bi.plano_conta pc&#xd;&#xa;                ON pc.exercicio &#x3d; rp_balancete.exercicio&#xd;&#xa;             WHERE pc.exercicio &#x3d; &#x27;&#x24;&#x7b;EXERCICIO&#x7d;&#x27;&#xd;&#xa;               AND pc.cod_estrutural SIMILAR TO &#x27;5.3.1.7.0.00.00.00.00.00&#x7c;6.3.1.7.1.00.00.00.00.00&#x27; and -1 &#x3c;&#x3e; &#x3f;&#x3b;&#xd;&#xa;&#xd;&#xa;ANALYZE bi.balancete_14&#x3b;</sql>
