    <sql>with &#xd;&#xa;a as&#xd;&#xa;&#x28;&#xd;&#xa;	-- gera linhas conforme &#x28;qtd_periodos -1&#x29;&#xd;&#xa;	select &#xd;&#xa;		row_number&#x28;&#x29; over&#x28;&#x29; -1 as m&#xd;&#xa;	from generate_series&#x28;1,&#xd;&#xa;							&#x28;		&#x28;      &#xd;&#xa;										-- quantidade de periodos entre periodos inicial e final&#xd;&#xa;										select &#xd;&#xa;											&#x28;&#xd;&#xa;												date_part&#x28;&#x27;month&#x27;,age&#x28;&#x27;&#x24;&#x7b;PERIODO_FINAL&#x7d;01&#x27;, &#x27;&#x24;&#x7b;PERIODO_INICIAL&#x7d;01&#x27;&#x29;&#x29; &#x2b; date_part&#x28;&#x27;year&#x27;,age&#x28;&#x27;&#x24;&#x7b;PERIODO_FINAL&#x7d;01&#x27;, &#x27;&#x24;&#x7b;PERIODO_INICIAL&#x7d;01&#x27;&#x29;&#x29; &#x2a; 12 &#x2b; 1 -- para periodos inicial e final distintos&#xd;&#xa;                                            &#x29; as qtd_periodos													 &#xd;&#xa;									&#x29;&#x3a;&#x3a;integer&#x29;			&#xd;&#xa;						&#x29; a&#xd;&#xa;&#x29;,&#xd;&#xa;b as&#xd;&#xa;&#x28;&#xd;&#xa;	-- transforma o numero da linha em string    &#xd;&#xa;	select &#xd;&#xa;		cast&#x28;b.m as char&#x28;3&#x29;&#x29; m&#xd;&#xa;	from a as b        &#xd;&#xa;&#x29;,&#xd;&#xa;c as&#xd;&#xa;&#x28;&#xd;&#xa;	-- com o uso do periodo inicial como referencia, calcula o periodo para cada linha&#xd;&#xa;	select &#xd;&#xa;		c.m, &#xd;&#xa;		date &#x27;&#x24;&#x7b;PERIODO_INICIAL&#x7d;01&#x27; &#x2b; &#x28;c.m &#x7c;&#x7c; &#x27; month&#x27;&#x29;&#x3a;&#x3a;INTERVAL as periodo&#xd;&#xa;	from b as c&#xd;&#xa;&#x29;,&#xd;&#xa;d as&#xd;&#xa;&#x28;&#xd;&#xa;	-- gera o exercicio e o mes&#xd;&#xa;	select &#xd;&#xa;	    d.periodo,&#xd;&#xa;		date_part&#x28;&#x27;year&#x27;,d.periodo&#x29; as EXERCICIO, &#xd;&#xa;		date_part&#x28;&#x27;month&#x27;,d.periodo&#x29; as MES &#xd;&#xa;	from c as d	&#xd;&#xa;	order by d.periodo&#xd;&#xa;&#x29;,&#xd;&#xa;e as&#xd;&#xa;&#x28;&#xd;&#xa;-- gera a ordem para o exercicio               &#xd;&#xa;	select &#xd;&#xa;		e.periodo,&#xd;&#xa;		e.EXERCICIO,&#xd;&#xa;		e.MES, &#xd;&#xa;		row_number&#x28;&#x29; over &#x28;partition by EXERCICIO order by periodo&#x29; ordem_exercicio &#xd;&#xa;	from d as e&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;-- REPROC_PRE_BALANCETE_EXERCICIO &#x3d; TRUE,  SER&#xc1; REPROCESSADO O PRE BALANCETE&#xd;&#xa;-- REPROC_PRE_BALANCETE_EXERCICIO &#x3d; FALSE, SER&#xc1; PROCESSADO  O PRE BALANCETE  SOMENTE SE N&#xc3;O HAVER REGISTRO EM bi.plano_conta&#xd;&#xa;select &#xd;&#xa;	 f.periodo&#xd;&#xa;	,f.EXERCICIO&#xd;&#xa;	,f.MES&#xd;&#xa;	, 	case &#xd;&#xa;			when  &#x28;true &#x3d; &#x24;&#x7b;REPROC_PRE_BALANCETE_EXERCICIO&#x7d; and f.ordem_exercicio &#x3d; 1&#x29; &#xd;&#xa;				then true&#xd;&#xa;			else &#xd;&#xa;			    false &#xd;&#xa;		end &#xd;&#xa;	as exec_recarga_pre_balancete&#xd;&#xa;from e as f&#x3b;</sql>
    <limit>0</limit>
