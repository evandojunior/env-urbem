      <sql>SET search_path &#x3d; &#x27;prestacao_contas&#x27;, pg_catalog&#x3b;&#xd;&#xa;&#xd;&#xa;create or replace function prestacao_contas.getVariablesKeyAndValue&#x28;varchar, varchar&#x29;&#xd;&#xa;	returns varchar&#xd;&#xa;	LANGUAGE plpgsql&#xd;&#xa;AS &#x24;function&#x24;&#xd;&#xa;declare&#xd;&#xa;    stKey alias for &#x24;1&#x3b;&#xd;&#xa;    stValue alias for &#x24;2&#x3b;&#xd;&#xa;    stResposta    varchar&#x3b;&#xd;&#xa;begin&#xd;&#xa;    stResposta &#x3a;&#x3d; stKey &#x7c;&#x7c; &#x27;&#x3a; &#x27; &#x7c;&#x7c; stValue &#x7c;&#x7c; &#x27;&#x3b;&#x5c;r&#x5c;n&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;    return stResposta&#x3b;&#xd;&#xa;END&#x3b;&#xd;&#xa;&#x24;function&#x24;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;STATUS PROCESSANDO&#x2a;&#x2f;&#xd;&#xa;create or replace function prestacao_contas.processa_status_processando&#x28;bigint&#x29;&#xd;&#xa;	returns void&#xd;&#xa;	LANGUAGE plpgsql&#xd;&#xa;AS &#x24;function&#x24;&#xd;&#xa;declare&#xd;&#xa;    inId alias for &#x24;1&#x3b;&#xd;&#xa;begin&#xd;&#xa;	update prestacao_contas.fila_relatorio &#xd;&#xa;	set &#xd;&#xa;	 status &#x3d; &#x27;PROCESSANDO&#x27;&#xd;&#xa;	,relatorio_log &#x3d; 	&#x28;	&#xd;&#xa;							select json_agg&#x28;value&#x29; from &#x28;&#xd;&#xa;							select &#x2a; from json_array_elements&#x28;&#x28;select relatorio_log from prestacao_contas.fila_relatorio where id &#x3d; inId&#x29;&#x3a;&#x3a;json&#x29; &#xd;&#xa;							union all&#xd;&#xa;							select &#x28;&#x27;&#x7b;&#x22;from&#x22;&#x3a;&#x22;ENVIADO&#x22;,&#x22;to&#x22;&#x3a;&#x22;PROCESSANDO&#x22;,&#x22;at&#x22;&#x3a;&#x22;&#x27; &#x7c;&#x7c; to_char&#x28;now&#x28;&#x29;,&#x27;yyyy-mm-dd HH24&#x3a;MI&#x3a;SS&#x27;&#x29; &#x7c;&#x7c;&#x27;&#x22;,&#x22;message&#x22;&#x3a;null&#x7d;&#x27;&#x29;&#x3a;&#x3a;json&#xd;&#xa;														&#x29; x &#xd;&#xa;						&#x29;&#xd;&#xa;	where id &#x3d; inId&#x3b;&#xd;&#xa;end&#x3b;&#xd;&#xa;&#x24;function&#x24;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;STATUS ERROR&#x2a;&#x2f;&#xd;&#xa;create or replace function prestacao_contas.processa_status_erro&#x28;inId bigint, varchar&#x29;&#xd;&#xa;	returns void&#xd;&#xa;	LANGUAGE plpgsql&#xd;&#xa;AS &#x24;function&#x24;&#xd;&#xa;declare&#xd;&#xa;    inId alias for &#x24;1&#x3b;&#xd;&#xa;    stMessage alias for &#x24;2&#x3b;&#xd;&#xa;begin&#xd;&#xa;    update prestacao_contas.fila_relatorio &#xd;&#xa;	    set &#xd;&#xa;	    	status &#x3d; &#x27;ERRO&#x27;&#xd;&#xa;			, data_resposta &#x3d; now&#x28;&#x29;&#x3a;&#x3a;timestamp&#x28;0&#x29;&#xd;&#xa;	    	, relatorio_log &#x3d; 	&#x28;	&#xd;&#xa;						select json_agg&#x28;value&#x29; from &#x28; &#xd;&#xa;						select &#x2a; from json_array_elements&#x28;&#x28;select relatorio_log from prestacao_contas.fila_relatorio where id &#x3d; inId&#x29;&#x3a;&#x3a;json&#x29; &#xd;&#xa;						union all&#xd;&#xa;						select &#x28;&#x27;&#x7b;&#x22;from&#x22;&#x3a;&#x22;PROCESSANDO&#x22;,&#x22;to&#x22;&#x3a;&#x22;ERRO&#x22;,&#x22;at&#x22;&#x3a;&#x22;&#x27; &#x7c;&#x7c; to_char&#x28;now&#x28;&#x29;,&#x27;yyyy-mm-dd HH24&#x3a;MI&#x3a;SS&#x27;&#x29; &#x7c;&#x7c;&#x27;&#x22;,&#x22;message&#x22;&#x3a;&#x22;ERRO AO INTERAGIR COM A BASE DE DADOS &#x5c;r&#x5c;n&#x27;  &#x7c;&#x7c; stMessage &#x7c;&#x7c; &#x27;&#x22;&#x7d;&#x27;&#x29;&#x3a;&#x3a;json &#xd;&#xa;													&#x29; x &#xd;&#xa;								&#x29;&#xd;&#xa;	    where id &#x3d; inId&#x3b;&#xd;&#xa;end&#x3b;&#xd;&#xa;&#x24;function&#x24;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;STATUS PRONTO&#x2a;&#x2f;&#xd;&#xa;create or replace function prestacao_contas.processa_status_pronto&#x28;bigint&#x29;&#xd;&#xa;	returns void&#xd;&#xa;	LANGUAGE plpgsql&#xd;&#xa;AS &#x24;function&#x24;&#xd;&#xa;declare&#xd;&#xa;    inId alias for &#x24;1&#x3b;&#xd;&#xa;begin&#xd;&#xa;	update prestacao_contas.fila_relatorio &#xd;&#xa;		set &#xd;&#xa;		 status &#x3d; &#x27;PRONTO&#x27;&#xd;&#xa;		,data_resposta &#x3d; now&#x28;&#x29;&#x3a;&#x3a;timestamp&#x28;0&#x29;&#xd;&#xa;		,relatorio_log &#x3d; 	&#x28;	&#xd;&#xa;								select json_agg&#x