    <entry>
      <name>SJ_010_ESS_CREATE_SCHEMA_FUNCTION</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;SET search_path &#x3d; &#x27;prestacao_contas&#x27;, pg_catalog&#x3b;&#xd;&#xa;&#xd;&#xa;CREATE OR REPLACE FUNCTION prestacao_contas.tcers_siapc_arq_aux&#x28;bigint&#x29;&#xd;&#xa; RETURNS text&#xd;&#xa; LANGUAGE plpgsql&#xd;&#xa;AS &#x24;function&#x24;&#xd;&#xa;DECLARE&#xd;&#xa;    inId            ALIAS FOR &#x24;1 &#x3b;&#xd;&#xa;    stParametros 	VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stEntidade		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stExercicio		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stMes			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	jbArquivos 		JSONB        &#x3b;&#xd;&#xa;	stSql     		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stSql2 			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stDtinicio		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stDtfim			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stCnpjSetor		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stNomeSetor		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stOrgao			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	reRegistro      RECORD       &#x3b;	&#xd;&#xa; 	stResultado		VARCHAR &#x3a;&#x3d; &#x27;ok&#x27;&#x3b;&#xd;&#xa;	stError			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;begin&#xd;&#xa;&#xd;&#xa;	&#xd;&#xa;	select &#xd;&#xa;		valor &#xd;&#xa;	into stParametros &#xd;&#xa;	from prestacao_contas.fila_relatorio &#xd;&#xa;	where id &#x3d; inId&#x3b;&#xd;&#xa;	&#xd;&#xa;	&#xd;&#xa;	stSql &#x3a;&#x3d; &#x27;select &#x2a; from json_to_record&#x28;&#x27;&#x27;&#x27;&#x7c;&#x7c;stParametros&#x7c;&#x7c;&#x27; &#x27;&#x27;&#x29;  as x&#x28;entidade text, mes int, exercicio text, arquivo text, data_inicio text, data_fim text, CnpjSetor text, St_Cnpj_Setor text, Orgao text&#x29;&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;	&#xd;&#xa;	FOR reRegistro IN EXECUTE stSql&#xd;&#xa;    LOOP		&#xd;&#xa;		stEntidade	&#x3a;&#x3d; reRegistro.entidade&#x3b;&#xd;&#xa;		stExercicio	&#x3a;&#x3d; reRegistro.exercicio&#x3b;&#xd;&#xa;		stMes		&#x3a;&#x3d; reRegistro.mes&#x3b;&#xd;&#xa;		jbArquivos 	&#x3a;&#x3d; &#x28;upper&#x28;reRegistro.arquivo&#x29;&#x29;&#x3a;&#x3a;jsonb&#x3b;&#xd;&#xa;		stDtinicio	&#x3a;&#x3d; reRegistro.data_inicio&#x3b;		&#xd;&#xa;		stDtfim		&#x3a;&#x3d; reRegistro.data_fim&#x3b;	&#xd;&#xa;		stCnpjSetor	&#x3a;&#x3d; reRegistro.CnpjSetor&#x3b;&#xd;&#xa;		stNomeSetor	&#x3a;&#x3d; reRegistro.St_Cnpj_Setor&#x3b;&#xd;&#xa;		stOrgao		&#x3a;&#x3d; reRegistro.Orgao&#x3b;&#xd;&#xa;    END LOOP&#x3b;&#xd;&#xa;&#xd;&#xa;    &#xd;&#xa;    CREATE TEMPORARY TABLE tmp_table &#x28;arquivo varchar&#x28;200&#x29;, content json&#x29;&#x3b;&#xd;&#xa;    &#xd;&#xa;    &#xd;&#xa;    &#xd;&#xa;    if jbArquivos &#x40;&#x3e; &#x27;&#x22;CREDOR.TXT&#x22;&#x27;&#x3a;&#x3a;jsonb&#xd;&#xa;	 	then  &#xd;&#xa;	 		&#xd;&#xa;			&#xd;&#xa;			with credor as&#xd;&#xa;			&#x28;&#xd;&#xa;			SELECT tbl.&#x2a; FROM&#x28; SELECT&#xd;&#xa;				    SW.numcgm,&#xd;&#xa;				    SW.nom_cgm,&#xd;&#xa;					CASE WHEN PF.numcgm IS NOT NULL THEN replace&#x28;replace&#x28;replace&#x28;PF.cpf,&#x27;-&#x27;,&#x27;&#x27;&#x29;,&#x27;&#x27;,&#x27;&#x27;&#x29;,&#x27; &#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;					ELSE replace&#x28;replace&#x28;replace&#x28;PJ.cnpj,&#x27;-&#x27;,&#x27;&#x27;&#x29;,&#x27;&#x27;,&#x27;&#x27;&#x29;, &#x27; &#x27;,&#x27;&#x27;&#x29; END AS cpf_cnpj,&#xd;&#xa;				    CASE&#xd;&#xa;				        WHEN PJ.insc_estadual &#x3d; &#x27;&#x27; THEN &#x27;0&#x27;&#xd;&#xa;				        ELSE replace&#x28;PJ.insc_estadual,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;				    END as insc_estadual,&#xd;&#xa;				    CASE&#xd;&#xa;				        WHEN max&#x28;EF.inscricao_economica&#x29; IS NOT NULL THEN max&#x28;replace&#x28;EF.inscricao_economica&#x3a;&#x3a;varchar,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#x3a;&#x3a;integer&#x29;&#xd;&#xa;				        WHEN max&#x28;ED.inscricao_economica&#x29; IS NOT NULL THEN max&#x28;replace&#x28;ED.inscricao_economica&#x3a;&#x3a;varchar,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#x3a;&#x3a;integer&#x29;&#xd;&#xa;				        WHEN max&#x28;EA.inscricao_economica&#x29; IS NOT NULL THEN max&#x28;replace&#x28;EA.inscricao_economica&#x3a;&#x3a;varchar,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#x3a;&#x3a;integer&#x29;&#xd;&#xa;				        ELSE NULL&#xd;&#xa;				    END AS insc_municipal,&#xd;&#xa;				                 CASE&#xd;&#xa;				                        WHEN max&#x28;EF.inscricao_economica&#x29; IS NOT NULL THEN max&#x28;EF.inscricao_economica&#x29;&#xd;&#xa;				                        WHEN max&#x28;ED.inscricao_economica&#x29; IS NOT NULL THEN max&#x28;ED.inscricao_economica&#x29;&#xd;&#xa;				                        WHEN max&#x28;EA.inscricao_economica&#x29; IS NOT NULL THEN max&#x28;EA.inscricao_economica&#x29;&#xd;&#xa;				                ELSE NULL                                                                                                             END AS inscricao_economica,&#xd;&#xa;				            sem_acentos&#x28;sw_tipo_logradouro.nom_tipo&#x29;&#x7c;&#x7c;&#x27; &#x27;&#x7c;&#x7c;sem_acentos&#x28;sw_nome_logradouro.nom_logradouro&#x29;&#x7c;&#x7c;&#x27; n&#x3a;&#x27;&#x7c;&#x7c;SW.numero&#x7c;&#x7c;&#x27; &#x27;&#x7c;&#x7c;sem_acentos&#x28;SW.complemento&#x29;&#x7c;&#x7c;&#x27; &#x27;&#x7c;&#x7c;sem_acentos&#x28;sw_bairro.nom_bairro&#x29;  AS endereco,&#xd;&#xa;				    SM.nom_municipio,&#xd;&#xa;				    SF.sigla_uf as nom_uf,&#xd;&#xa;				    sw_cgm_logradouro.cep,&#xd;&#xa;				    SW.fone_comercial,&#xd;&#xa;				    &#x27;&#x27; AS fax,&#xd;&#xa;				    TC.tipo&#xd;&#xa;				FROM&#xd;&#xa;				    sw_municipio            AS SM,&#xd;&#xa;				    sw_uf                   AS SF,&#xd;&#xa;				    tcers.credor            AS TC,&#xd;&#xa;				    &#x28;select max&#x28;exercicio&#x29; as exercicio,numcgm from tcers.credor group by numcgm &#x29; as TC2,&#xd;&#xa;				    sw_cgm                  AS SW&#xd;&#xa;				LEFT JOIN&#xd;&#xa;				    sw_cgm_pessoa_fisica    AS PF&#xd;&#xa;				ON&#xd;&#xa;				    SW.numcgm     &#x3d; PF.numcgm&#xd;&#xa;				LEFT JOIN&#xd;&#xa;				    sw_cgm_pessoa_juridica    AS PJ&#xd;&#xa;				ON&#xd;&#xa;				    SW.numcgm     &#x3d; PJ.numcgm&#xd;&#xa;				LEFT JOIN&#xd;&#xa;				    sw_cgm_logradouro&#xd;&#xa;				ON&#xd;&#xa;				    sw_cgm_logradouro.numcgm &#x3d; SW.numcgm&#xd;&#xa;				LEFT JOIN&#xd;&#xa;				    sw_nome_logradouro&#xd;&#xa;				ON&#xd;&#xa;				    sw_nome_logradouro.cod_logradouro &#x3d; sw_cgm_logradouro.cod_logradouro&#xd;&#xa;				AND sw_nome_logradouro.timestamp &#x3d; &#x28; SELECT MAX&#x28;timestamp&#x29; FROM sw_nome_logradouro AS logradouro WHERE logradouro.cod_logradouro &#x3d; sw_nome_logradouro.cod_logradouro &#x29;&#xd;&#xa;				LEFT JOIN&#xd;&#xa;				    sw_tipo_logradouro&#xd;&#xa;				ON&#xd;&#xa;				    sw_tipo_logradouro.cod_tipo &#x3d; sw_nome_logradouro.cod_tipo&#xd;&#xa;				LEFT JOIN&#xd;&#xa;				    sw_bairro&#xd;&#xa;				ON&#x28;&#xd;&#xa;				    sw_bairro.cod_bairro &#x3d; sw_cgm_logradouro.cod_bairro&#xd;&#xa;				AND&#xd;&#xa;				    sw_bairro.cod_municipio &#x3d; sw_cgm_logradouro.cod_municipio&#xd;&#xa;				AND&#xd;&#xa;				    sw_bairro.cod_uf &#x3d; sw_cgm_logradouro.cod_uf&#x29;&#xd;&#xa;				LEFT JOIN&#xd;&#xa;				    economico.cadastro_economico_empresa_fato AS EF&#xd;&#xa;				ON&#xd;&#xa;				    EF.numcgm               &#x3d; SW.numcgm&#xd;&#xa;				LEFT JOIN&#xd;&#xa;				    economico.cadastro_economico_empresa_direito AS ED&#xd;&#xa;				ON&#xd;&#xa;				    ED.numcgm               &#x3d; SW.numcgm&#xd;&#xa;				LEFT JOIN&#xd;&#xa;				    economico.cadastro_economico_autonomo AS EA&#xd;&#xa;				ON&#xd;&#xa;				    EA.numcgm               &#x3d; SW.numcgm&#xd;&#xa;				WHERE&#xd;&#xa;				    SW.numcgm               &#x3d; TC.numcgm AND&#xd;&#xa;				    SW.cod_municipio        &#x3d; SM.cod_municipio AND&#xd;&#xa;				    SW.cod_uf               &#x3d; SM.cod_uf AND&#xd;&#xa;				    SM.cod_uf               &#x3d; SF.cod_uf AND&#xd;&#xa;				    TC.exercicio            &#x3d; TC2.exercicio AND&#xd;&#xa;				    TC.numcgm               &#x3d; TC2.numcgm AND&#xd;&#xa;				    TC.numcgm in&#xd;&#xa;				        &#x28;SELECT&#xd;&#xa;				            EP.cgm_beneficiario&#xd;&#xa;				        FROM&#xd;&#xa;				            empenho.empenho         AS EE,&#xd;&#xa;				            empenho.pre_empenho     AS EP&#xd;&#xa;				        WHERE&#xd;&#xa;				            EE.exercicio            &#x3d; EP.exercicio AND&#xd;&#xa;				            EE.cod_pre_empenho      &#x3d; EP.cod_pre_empenho&#x29;&#xd;&#xa;				  GROUP BY&#xd;&#xa;				  SW.numcgm,&#xd;&#xa;				  SW.nom_cgm,&#xd;&#xa;				  cpf_cnpj,&#xd;&#xa;				  insc_estadual,&#xd;&#xa;				  endereco,&#xd;&#xa;				  sw_cgm_logradouro.cod_logradouro,&#xd;&#xa;				  SM.nom_municipio,&#xd;&#xa;				  SF.sigla_uf,&#xd;&#xa;				  sw_cgm_logradouro.cep,&#xd;&#xa;				  SW.fone_comercial,&#xd;&#xa;				  fax,&#xd;&#xa;				  TC.tipo&#xd;&#xa;				 ORDER BY numcgm ASC &#x29; as tbl&#xd;&#xa;			&#x29;&#xd;&#xa;			, credor_format as &#xd;&#xa;			&#x28;&#xd;&#xa;				select &#xd;&#xa;					   &#x28;stCnpjSetor    &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtinicio, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29; &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtfim, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;  to_char&#x28;now&#x28;&#x29;, &#x27;ddmmyyyy&#x27;&#x29;					   &#xd;&#xa;					&#x7c;&#x7c;  rpad&#x28; upper&#x28;stNomeSetor&#x29;, 80, &#x27; &#x27;&#x29; &#x29;  as registro_credor					&#xd;&#xa;				union all&#xd;&#xa;  			    select&#xd;&#xa;  			       &#xd;&#xa;				     lpad&#x28;coalesce&#x28;numcgm&#x3a;&#x3a;varchar, &#x27;0&#x27;&#x29;, 10, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; rpad&#x28;coalesce&#x28;nom_cgm&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 60, &#x27; &#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;cpf_cnpj&#x3a;&#x3a;varchar, &#x27;0&#x27;&#x29;, 14, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;insc_estadual&#x3a;&#x3a;varchar, &#x27;0&#x27;&#x29;, 15, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;insc_municipal&#x3a;&#x3a;varchar, &#x27;0&#x27;&#x29;, 15, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; rpad&#x28;coalesce&#x28;endereco&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 50, &#x27; &#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; rpad&#x28;coalesce&#x28;nom_municipio&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 30, &#x27; &#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; rpad&#x28;coalesce&#x28;nom_uf&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 2, &#x27; &#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;cep&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 8, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;fone_comercial&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 15, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;fax&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 15, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;tipo&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 2, &#x27;0&#x27;&#x29;&#xd;&#xa;				as registro_credor&#xd;&#xa;				from credor&#xd;&#xa;				union all&#xd;&#xa;				select &#x27;FINALIZADOR&#x27;&#x7c;&#x7c;&#x28;select lpad&#x28;count&#x28;&#x2a;&#x29;&#x3a;&#x3a;varchar,10,&#x27;0&#x27;&#x29; from credor&#x29;		&#xd;&#xa;			&#x29;&#xd;&#xa;			insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;				select  &#x27;CREDOR.TXT&#x27;, json_agg&#x28;     regexp_replace&#x28;  registro_credor  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;     &#x29; from credor_format&#x3b;   &#xd;&#xa;    end if&#x3b;		&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;    if jbArquivos &#x40;&#x3e; &#x27;&#x22;FUNCAO.TXT&#x22;&#x27;&#x3a;&#x3a;jsonb&#xd;&#xa;	 	then      &#xd;&#xa;	 		&#xd;&#xa;			&#xd;&#xa;			with funcao as&#xd;&#xa;			&#x28;&#xd;&#xa;			SELECT&#xd;&#xa;				     OF.exercicio,&#xd;&#xa;				     OF.cod_funcao,&#xd;&#xa;				     OF.descricao&#xd;&#xa;				  FROM&#xd;&#xa;				     orcamento.funcao AS OF&#xd;&#xa;				  WHERE&#xd;&#xa;				     OF.exercicio &#x3c;&#x3d; stExercicio&#xd;&#xa;				  UNION&#xd;&#xa;				  SELECT&#xd;&#xa;				     &#x27;2004&#x27; as exercicio,&#xd;&#xa;				     OF.cod_funcao,&#xd;&#xa;				     &#x27;FUNCAO&#x27; as descricao&#xd;&#xa;				  FROM&#xd;&#xa;				     orcamento.funcao AS OF&#xd;&#xa;				  WHERE&#xd;&#xa;				     OF.exercicio &#x3d; &#x27;2005&#x27;&#xd;&#xa;				 ORDER BY exercicio DESC&#xd;&#xa;			&#x29;&#xd;&#xa;			, funcao_format as &#xd;&#xa;			&#x28;&#xd;&#xa;			  select &#xd;&#xa;				   &#x28;stCnpjSetor    &#xd;&#xa;				&#x7c;&#x7c;  replace&#x28;stDtinicio, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29; &#xd;&#xa;				&#x7c;&#x7c;  replace&#x28;stDtfim, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;				&#x7c;&#x7c;  to_char&#x28;now&#x28;&#x29;, &#x27;ddmmyyyy&#x27;&#x29;					   &#xd;&#xa;				&#x7c;&#x7c;  rpad&#x28; upper&#x28;stNomeSetor&#x29;, 80, &#x27; &#x27;&#x29; &#x29;  as registro_funcao					&#xd;&#xa;			  union all&#xd;&#xa;			  select&#xd;&#xa;			     lpad&#x28;coalesce&#x28;exercicio&#x3a;&#x3a;varchar, &#x27;0&#x27;&#x29;, 4, &#x27;0&#x27;&#x29;&#xd;&#xa;			  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_funcao&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 2, &#x27;0&#x27;&#x29;&#xd;&#xa;			  &#x7c;&#x7c; rpad&#x28;coalesce&#x28;descricao&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 80, &#x27; &#x27;&#x29;&#xd;&#xa;			  as registro_funcao&#xd;&#xa;			  from funcao&#xd;&#xa;			  union all&#xd;&#xa;			  select &#x27;FINALIZADOR&#x27;&#x7c;&#x7c;&#x28;select lpad&#x28;count&#x28;&#x2a;&#x29;&#x3a;&#x3a;varchar,10,&#x27;0&#x27;&#x29; from funcao&#x29;&#xd;&#xa;			&#x29;&#xd;&#xa;			insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;				select  &#x27;FUNCAO.TXT&#x27;, json_agg&#x28;     regexp_replace&#x28;  registro_funcao  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;     &#x29; from funcao_format&#x3b;   &#xd;&#xa;    end if&#x3b;		&#xd;&#xa;			&#xd;&#xa;    if jbArquivos &#x40;&#x3e; &#x27;&#x22;ORGAO.TXT&#x22;&#x27;&#x3a;&#x3a;jsonb&#xd;&#xa;	 	then      			&#xd;&#xa;	 		&#xd;&#xa;			&#xd;&#xa;			with orgao as&#xd;&#xa;			&#x28;&#xd;&#xa;				SELECT &#x2a; FROM &#x28;&#xd;&#xa;					SELECT&#xd;&#xa;					     OO.exercicio,&#xd;&#xa;					     OO.num_orgao,&#xd;&#xa;					     OO.nom_orgao&#xd;&#xa;					FROM&#xd;&#xa;					     orcamento.orgao AS OO&#xd;&#xa;					WHERE&#xd;&#xa;					     OO.exercicio  &#x3c;&#x3d; stExercicio&#xd;&#xa;					UNION&#xd;&#xa;					SELECT&#xd;&#xa;					     &#x27;2004&#x27; as exercicio,&#xd;&#xa;					     OO.num_orgao,&#xd;&#xa;					     &#x27;ORGAO&#x27; as nom_orgao&#xd;&#xa;					FROM&#xd;&#xa;					     orcamento.orgao AS OO&#xd;&#xa;					WHERE&#xd;&#xa;					     OO.exercicio &#x3d; &#x27;2005&#x27;&#xd;&#xa;					&#x29; as tabela&#xd;&#xa;					 ORDER BY exercicio DESC&#xd;&#xa;			&#x29;&#xd;&#xa;			, orgao_format as &#xd;&#xa;			&#x28;&#xd;&#xa;				  select &#xd;&#xa;					   &#x28;stCnpjSetor    &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtinicio, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29; &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtfim, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;  to_char&#x28;now&#x28;&#x29;, &#x27;ddmmyyyy&#x27;&#x29;					   &#xd;&#xa;					&#x7c;&#x7c;  rpad&#x28; upper&#x28;stNomeSetor&#x29;, 80, &#x27; &#x27;&#x29; &#x29;  as registro_orgao					&#xd;&#xa;				  union all&#xd;&#xa;				  select&#xd;&#xa;				     lpad&#x28;coalesce&#x28;exercicio&#x3a;&#x3a;varchar, &#x27;0&#x27;&#x29;, 4, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;num_orgao&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 2, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; rpad&#x28;coalesce&#x28;nom_orgao&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 80, &#x27; &#x27;&#x29;&#xd;&#xa;				  as registro_orgao&#xd;&#xa;				  from orgao&#xd;&#xa;			      union all&#xd;&#xa;			      select &#x27;FINALIZADOR&#x27;  &#x7c;&#x7c; lpad&#x28;&#x28;&#x28;select COUNT&#x28;&#x2a;&#x29;&#x3a;&#x3a;varchar  from orgao&#x29;&#x3a;&#x3a;varchar&#x29;, 10, &#x27;0&#x27;&#x29;  as registro_orgao&#xd;&#xa;			&#x29;&#xd;&#xa;				 