x27; &#x27;, &#x27;g&#x27;&#x29;     &#x29; from projativ_format&#x3b;   &#xd;&#xa;    end if&#x3b;	  &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;    if jbArquivos &#x40;&#x3e; &#x27;&#x22;RECURSO.TXT&#x22;&#x27;&#x3a;&#x3a;jsonb&#xd;&#xa;	 	then      &#xd;&#xa;			&#xd;&#xa;			&#xd;&#xa;			with recurso as&#xd;&#xa;			&#x28;&#xd;&#xa;				SELECT&#xd;&#xa;				     RO.cod_recurso,&#xd;&#xa;				     RO.nom_recurso,&#xd;&#xa;				     RO.finalidade,&#xd;&#xa;				     RO.tipo&#xd;&#xa;				FROM&#xd;&#xa;				     orcamento.recurso&#x28; stExercicio &#x29; AS RO&#xd;&#xa;				ORDER BY RO.cod_recurso&#xd;&#xa;			&#x29;&#xd;&#xa;			, recurso_format as &#xd;&#xa;			&#x28;&#xd;&#xa;			  	select &#xd;&#xa;					   &#x28;stCnpjSetor    &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtinicio, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29; &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtfim, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;  to_char&#x28;now&#x28;&#x29;, &#x27;ddmmyyyy&#x27;&#x29;					   &#xd;&#xa;					&#x7c;&#x7c;  rpad&#x28; upper&#x28;stNomeSetor&#x29;, 80, &#x27; &#x27;&#x29; &#x29;  as registro_recurso					&#xd;&#xa;			  	union all&#xd;&#xa;			  	select&#xd;&#xa;					   lpad&#x28;coalesce&#x28;cod_recurso&#x3a;&#x3a;varchar, &#x27;0&#x27;&#x29;, 4, &#x27;0&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c; rpad&#x28;coalesce&#x28;nom_recurso&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 80, &#x27; &#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c; rpad&#x28;coalesce&#x28;finalidade&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 160, &#x27; &#x27;&#x29;	  &#xd;&#xa;				as registro_recurso&#xd;&#xa;			  	from recurso&#xd;&#xa;			  	union all&#xd;&#xa;			  	select &#x27;FINALIZADOR&#x27;  &#x7c;&#x7c; lpad&#x28;&#x28;&#x28;select COUNT&#x28;&#x2a;&#x29;&#x3a;&#x3a;varchar  from recurso&#x29;&#x3a;&#x3a;varchar&#x29;, 10, &#x27;0&#x27;&#x29;  as registro_recurso&#xd;&#xa;			&#x29;&#xd;&#xa;				insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;				select  &#x27;RECURSO.TXT&#x27;, json_agg&#x28;     regexp_replace&#x28;  registro_recurso  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;     &#x29; from recurso_format&#x3b;   &#xd;&#xa;    end if&#x3b;&#xd;&#xa;	&#xd;&#xa;&#xd;&#xa;    if jbArquivos &#x40;&#x3e; &#x27;&#x22;RUBRICA.TXT&#x22;&#x27;&#x3a;&#x3a;jsonb&#xd;&#xa;	 	then      &#xd;&#xa;			&#xd;&#xa;			&#xd;&#xa;			with rubrica as&#xd;&#xa;			&#x28;&#xd;&#xa;				SELECT &#x2a; FROM &#x28;&#xd;&#xa;					  SELECT&#xd;&#xa;					     OD.exercicio,&#xd;&#xa;					     replace&#x28;OD.cod_estrutural,&#x27;.&#x27;,&#x27;&#x27;&#x29; as cod_estrutural,&#xd;&#xa;					     OD.descricao,&#xd;&#xa;					     tcers.tipo_conta_rubrica&#x28;&#x27;OD.exercicio&#x27;,&#x27;OD.cod_estrutural&#x27;&#x29; as tnc,&#xd;&#xa;					     publico.fn_nivel&#x28;OD.cod_estrutural&#x29; as nnc&#xd;&#xa;					  FROM&#xd;&#xa;					     orcamento.conta_despesa AS OD&#xd;&#xa;					  WHERE&#xd;&#xa;					     OD.exercicio &#x3c;&#x3d; stExercicio&#xd;&#xa;					&#x29; AS tabela&#xd;&#xa;					 ORDER BY exercicio DESC&#xd;&#xa;			&#x29;&#xd;&#xa;			, rubrica_format as &#xd;&#xa;			&#x28;&#xd;&#xa;				select &#xd;&#xa;					   &#x28;stCnpjSetor    &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtinicio, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29; &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtfim, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;  to_char&#x28;now&#x28;&#x29;, &#x27;ddmmyyyy&#x27;&#x29;					   &#xd;&#xa;					&#x7c;&#x7c;  rpad&#x28; upper&#x28;stNomeSetor&#x29;, 80, &#x27; &#x27;&#x29; &#x29;  as registro_rubrica					&#xd;&#xa;			  	union all&#xd;&#xa;				select&#xd;&#xa;				     lpad&#x28;coalesce&#x28;exercicio&#x3a;&#x3a;varchar, &#x27;0&#x27;&#x29;, 4, &#x27;0&#x27;&#x29;&#xd;&#xa;			      &#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_estrutural&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 15, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; rpad&#x28;coalesce&#x28;descricao&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 110, &#x27; &#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; rpad&#x28;coalesce&#x28;tnc&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 1, &#x27; &#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;nnc&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 2, &#x27;0&#x27;&#x29;	  &#xd;&#xa;				as registro_rubrica&#xd;&#xa;				from rubrica&#xd;&#xa;			    union all&#xd;&#xa;			    select &#x27;FINALIZADOR&#x27;  &#x7c;&#x7c; lpad&#x28;&#x28;&#x28;select COUNT&#x28;&#x2a;&#x29;&#x3a;&#x3a;varchar  from rubrica&#x29;&#x3a;&#x3a;varchar&#x29;, 10, &#x27;0&#x27;&#x29;  as registro_rubrica&#xd;&#xa;			&#x29;&#xd;&#xa;				insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;				select  &#x27;RUBRICA.TXT&#x27;, json_agg&#x28;     regexp_replace&#x28;  registro_rubrica  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;     &#x29; from rubrica_format&#x3b;   &#xd;&#xa;    end if&#x3b;&#xd;&#xa;&#xd;&#xa;    if jbArquivos &#x40;&#x3e; &#x27;&#x22;SUBFUNC.TXT&#x22;&#x27;&#x3a;&#x3a;jsonb&#xd;&#xa;	 	then      &#xd;&#xa;			&#xd;&#xa;			&#xd;&#xa;			with subfunc as&#xd;&#xa;			&#x28;&#xd;&#xa;				SELECT &#x2a; FROM &#x28;&#xd;&#xa;					  SELECT&#xd;&#xa;					     OS.exercicio,&#xd;&#xa;					     OS.cod_subfuncao,&#xd;&#xa;					     OS.descricao&#xd;&#xa;					  FROM&#xd;&#xa;					     orcamento.subfuncao AS OS&#xd;&#xa;					  WHERE&#xd;&#xa;					     OS.exercicio &#x3c;&#x3d; stExercicio&#xd;&#xa;					UNION&#xd;&#xa;					  SELECT&#xd;&#xa;					     &#x27;2004&#x27; as exercicio,&#xd;&#xa;					     OS.cod_subfuncao,&#xd;&#xa;					     &#x27;SUBFUNCAO&#x27; as descricao&#xd;&#xa;					  FROM&#xd;&#xa;					     orcamento.subfuncao AS OS&#xd;&#xa;					  WHERE exercicio &#x3d; &#x27;2005&#x27;&#xd;&#xa;					&#x29; AS tabela&#xd;&#xa;					 ORDER BY exercicio DESC&#xd;&#xa;			&#x29;&#xd;&#xa;			, subfunc_format as &#xd;&#xa;			&#x28;&#xd;&#xa;				select &#xd;&#xa;					   &#x28;stCnpjSetor    &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtinicio, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29; &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtfim, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;  to_char&#x28;now&#x28;&#x29;, &#x27;ddmmyyyy&#x27;&#x29;					   &#xd;&#xa;					&#x7c;&#x7c;  rpad&#x28; upper&#x28;stNomeSetor&#x29;, 80, &#x27; &#x27;&#x29; &#x29;  as registro_subfunc					&#xd;&#xa;			  	union all&#xd;&#xa;				select&#xd;&#xa;				     lpad&#x28;coalesce&#x28;exercicio&#x3a;&#x3a;varchar, &#x27;0&#x27;&#x29;, 4, &#x27;0&#x27;&#x29;&#xd;&#xa;			      &#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_subfuncao&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 3, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; rpad&#x28;coalesce&#x28;descricao&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 80, &#x27; &#x27;&#x29;&#xd;&#xa;				as registro_subfunc&#xd;&#xa;				from subfunc&#xd;&#xa;			    union all&#xd;&#xa;			    select &#x27;FINALIZADOR&#x27;  &#x7c;&#x7c; lpad&#x28;&#x28;&#x28;select COUNT&#x28;&#x2a;&#x29;&#x3a;&#x3a;varchar  from subfunc&#x29;&#x3a;&#x3a;varchar&#x29;, 10, &#x27;0&#x27;&#x29;  as registro_subfunc&#xd;&#xa;			&#x29;&#xd;&#xa;				insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;				select  &#x27;SUBFUNC.TXT&#x27;, json_agg&#x28;     regexp_replace&#x28;  registro_subfunc  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;     &#x29; from subfunc_format&#x3b;   &#xd;&#xa;    end if&#x3b;&#xd;&#xa;&#xd;&#xa;    if jbArquivos &#x40;&#x3e; &#x27;&#x22;SUBPROG.TXT&#x22;&#x27;&#x3a;&#x3a;jsonb&#xd;&#xa;	 	then      &#xd;&#xa;			&#xd;&#xa;			&#xd;&#xa;			with subprog as&#xd;&#xa;			&#x28;&#xd;&#xa;				SELECT&#xd;&#xa;					     ep.exercicio,&#xd;&#xa;					     &#x27;&#x27; as cod_subprograma,&#xd;&#xa;					     &#x27;SUBPROGRAMA&#x27; as nom_subprograma&#xd;&#xa;				FROM&#xd;&#xa;					     empenho.pre_empenho AS ep&#xd;&#xa;				WHERE ep.exercicio &#x3c;&#x3d; stExercicio&#xd;&#xa;				UNION&#xd;&#xa;				SELECT&#xd;&#xa;					     ee.exercicio,&#xd;&#xa;					     &#x27;&#x27; as cod_subprograma,&#