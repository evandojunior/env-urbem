    <entry>
      <name>SJ_010_ESS_CREATE_SCHEMA_FUNCTION</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;SET search_path &#x3d; &#x27;prestacao_contas&#x27;, pg_catalog&#x3b;&#xd;&#xa;&#xd;&#xa;CREATE OR REPLACE FUNCTION prestacao_contas.tcers_siapc_arq_disp&#x28;bigint&#x29;&#xd;&#xa; RETURNS text&#xd;&#xa; LANGUAGE plpgsql&#xd;&#xa;AS &#x24;function&#x24;&#xd;&#xa;DECLARE&#xd;&#xa;    inId            ALIAS FOR &#x24;1 &#x3b;&#xd;&#xa;    stParametros 	VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stEntidade		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stExercicio		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stMes			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	jbArquivos 		JSONB        &#x3b;&#xd;&#xa;	stSql     		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stSql2 			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stDtinicio		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stDtfim			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stCnpjSetor		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stNomeSetor		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stOrgao			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	reRegistro      RECORD       &#x3b;	&#xd;&#xa; 	stResultado		VARCHAR &#x3a;&#x3d; &#x27;ok&#x27;&#x3b;&#xd;&#xa;	stError			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b; 	&#xd;&#xa;begin&#xd;&#xa;&#xd;&#xa;	&#xd;&#xa;	select &#xd;&#xa;		valor &#xd;&#xa;	into stParametros &#xd;&#xa;	from prestacao_contas.fila_relatorio &#xd;&#xa;	where id &#x3d; inId&#x3b;&#xd;&#xa;	&#xd;&#xa;	&#xd;&#xa;	stSql &#x3a;&#x3d; &#x27;select &#x2a; from json_to_record&#x28;&#x27;&#x27;&#x27;&#x7c;&#x7c;stParametros&#x7c;&#x7c;&#x27;	&#x27;&#x27;&#x29;  as x&#x28;entidade text, mes int, exercicio text, arquivo text, data_inicio text, data_fim text, CnpjSetor text, St_Cnpj_Setor text, Orgao text &#x29;&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;	&#xd;&#xa;	FOR reRegistro IN EXECUTE stSql&#xd;&#xa;    LOOP		&#xd;&#xa;		stEntidade	&#x3a;&#x3d; reRegistro.entidade&#x3b;&#xd;&#xa;		stExercicio	&#x3a;&#x3d; reRegistro.exercicio&#x3b;&#xd;&#xa;		stMes		&#x3a;&#x3d; reRegistro.mes&#x3b;&#xd;&#xa;		jbArquivos 	&#x3a;&#x3d; &#x28;upper&#x28;reRegistro.arquivo&#x29;&#x29;&#x3a;&#x3a;jsonb&#x3b;&#xd;&#xa;		stDtinicio	&#x3a;&#x3d; reRegistro.data_inicio&#x3b;		&#xd;&#xa;		stDtfim		&#x3a;&#x3d; reRegistro.data_fim&#x3b;		&#xd;&#xa;		stCnpjSetor	&#x3a;&#x3d; reRegistro.CnpjSetor&#x3b;&#xd;&#xa;		stNomeSetor	&#x3a;&#x3d; reRegistro.St_Cnpj_Setor&#x3b;&#xd;&#xa;		stOrgao		&#x3a;&#x3d; reRegistro.Orgao&#x3b;&#xd;&#xa;    END LOOP&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;    CREATE TEMPORARY TABLE tmp_table &#x28;arquivo varchar&#x28;200&#x29;, content json&#x29;&#x3b;&#xd;&#xa;    &#xd;&#xa;    &#xd;&#xa;    &#xd;&#xa;    if jbArquivos &#x40;&#x3e; &#x27;&#x22;TCE_4111.TXT&#x22;&#x27;&#x3a;&#x3a;jsonb&#xd;&#xa;	 	then  &#xd;&#xa;	 		&#xd;&#xa;			&#xd;&#xa;			with TCE_4111 as&#xd;&#xa;			&#x28;&#xd;&#xa;				SELECT &#x2a; FROM &#xd;&#xa;				&#x28;&#xd;&#xa;						select &#xd;&#xa;								pa.cod_conta,&#xd;&#xa;								pa.cod_plano,&#xd;&#xa;								pa.exercicio,&#xd;&#xa;								pa.natureza_saldo,&#xd;&#xa;								pc.atributo_tcemg,&#xd;&#xa;								pc.atributo_tcepe,&#xd;&#xa;								pc.cod_classificacao,&#xd;&#xa;								pc.cod_estrutural,&#xd;&#xa;								pc.cod_sistema,&#xd;&#xa;								pc.escrituracao,&#xd;&#xa;								pc.escrituracao_pcasp,&#xd;&#xa;								pc.funcao,&#xd;&#xa;								pc.indicador_superavit,&#xd;&#xa;								pc.nom_conta,&#xd;&#xa;								pc.obrigatorio_tcmgo,&#xd;&#xa;								cd.cod_entidade,&#xd;&#xa;								cd.cod_lote,&#xd;&#xa;								cd.sequencia,&#xd;&#xa;								cd.tipo,&#xd;&#xa;								cd.tipo_valor,&#xd;&#xa;								vl.oid_lancamento,&#xd;&#xa;								vl.vl_lancamento,&#xd;&#xa;								la.complemento,&#xd;&#xa;								la.cod_historico,&#xd;&#xa;								chc.historico_interno,&#xd;&#xa;								chc.nom_historico,&#xd;&#xa;								le.estorno,&#xd;&#xa;								cl.cod_nota,&#xd;&#xa;								cl.exercicio_liquidacao,&#xd;&#xa;								enl.cod_empenho,&#xd;&#xa;								enl.dt_liquidacao,&#xd;&#xa;								enl.dt_vencimento,&#xd;&#xa;								enl.exercicio_empenho,&#xd;&#xa;								enl.hora,&#xd;&#xa;								enl.observacao,&#xd;&#xa;								ee.cod_categoria,&#xd;&#xa;								ee.cod_pre_empenho,&#xd;&#xa;								ee.dt_empenho,&#xd;&#xa;								ee.restos_pagar,&#xd;&#xa;								ee.vl_saldo_anterior, &#xd;&#xa;								&#x27;D&#x27; as tipo_conta1&#xd;&#xa;						FROM&#xd;&#xa;						             contabilidade.plano_conta            as pc&#xd;&#xa;						            ,contabilidade.plano_analitica        as pa&#xd;&#xa;						            ,contabilidade.conta_debito           as cd&#xd;&#xa;						            ,contabilidade.valor_lancamento       as vl&#xd;&#xa;						            ,contabilidade.lancamento             as la&#xd;&#xa;						LEFT JOIN    contabilidade.historico_contabil as chc&#xd;&#xa;						       ON    chc.exercicio &#x3d; la.exercicio&#xd;&#xa;						      AND    chc.cod_historico &#x3d; la.cod_historico&#xd;&#xa;						LEFT JOIN    contabilidade.lancamento_empenho     as le&#xd;&#xa;						  	   ON 	 le.exercicio &#x3d; la.exercicio&#xd;&#xa;						 	  AND 	 le.cod_entidade &#x3d; la.cod_entidade&#xd;&#xa;						 	  AND 	 le.tipo &#x3d; la.tipo&#xd;&#xa;						 	  AND 	 le.cod_lote &#x3d; la.cod_lote&#xd;&#xa;						 	  AND 	 le.sequencia &#x3d; la.sequencia&#xd;&#xa;						LEFT JOIN 	 contabilidade.liquidacao as cl&#xd;&#xa;						  	   ON 	 le.exercicio &#x3d; cl.exercicio&#xd;&#xa;						      AND    le.cod_lote &#x3d; cl.cod_lote&#xd;&#xa;						 	  AND    le.tipo &#x3d; cl.tipo&#xd;&#xa;						 	  AND    le.sequencia &#x3d; cl.sequencia&#xd;&#xa;						 	  AND    le.cod_entidade &#x3d; cl.cod_entidade&#xd;&#xa;						LEFT JOIN    empenho.nota_liquidacao as enl&#xd;&#xa;						  ON cl.exercicio &#x3d; enl.exercicio&#xd;&#xa;						 AND cl.cod_entidade &#x3d; enl.cod_entidade&#xd;&#xa;						 AND cl.cod_nota &#x3d; enl.cod_nota&#xd;&#xa;						LEFT JOIN empenho.empenho as ee&#xd;&#xa;						  ON ee.exercicio &#x3d; enl.exercicio&#xd;&#xa;						 AND ee.cod_entidade &#x3d; enl.cod_entidade&#xd;&#xa;						 AND ee.cod_empenho &#x3d; enl.cod_empenho&#xd;&#xa;						            ,contabilidade.lote                   as lo&#xd;&#xa;						            ,contabilidade.sistema_contabil       as sc&#xd;&#xa;						        WHERE   pc.cod_conta    &#x3d; pa.cod_conta&#xd;&#xa;						        AND     pc.exercicio    &#x3d; pa.exercicio&#xd;&#xa;						        AND     pa.cod_plano    &#x3d; cd.cod_plano&#xd;&#xa;						        AND     pa.exercicio    &#x3d; cd.exercicio&#xd;&#xa;						        AND     cd.cod_lote     &#x3d; vl.cod_lote&#xd;&#xa;						        AND     cd.tipo         &#x3d; vl.tipo&#xd;&#xa;						        AND     cd.sequencia    &#x3d; vl.sequencia&#xd;&#xa;						        AND     cd.exercicio    &#x3d; vl.exercicio&#xd;&#xa;						        AND     cd.tipo_valor   &#x3d; vl.tipo_valor&#xd;&#xa;						        AND     cd.cod_entidade &#x3d; vl.cod_entidade&#xd;&#xa;						        AND     vl.cod_lote     &#x3d; la.cod_lote&#xd;&#xa;						        AND     vl.tipo         &#x3d; la.tipo&#xd;&#xa;						        AND     vl.sequencia    &#x3d; la.sequencia&#xd;&#xa;						        AND     vl.exercicio    &#x3d; la.exercicio&#xd;&#xa;						        AND     vl.cod_entidade &#x3d; la.cod_entidade&#xd;&#xa;						        AND     la.cod_lote     &#x3d; lo.cod_lote&#xd;&#xa;						        AND     la.exercicio    &#x3d; lo.exercicio&#xd;&#xa;						        AND     la.tipo         &#x3d; lo.tipo&#xd;&#xa;						        AND     la.cod_entidade &#x3d; lo.cod_entidade&#xd;&#xa;						        AND     pa.exercicio &#x3d;  stExercicio   &#xd;&#xa;						        AND     sc.cod_sistema  &#x3d; pc.cod_sistema&#xd;&#xa;						        AND     sc.exercicio    &#x3d; pc.exercicio&#xd;&#xa;						AND     lo.dt_lote between to_date&#x28;stDtinicio,&#x27;dd&#x2f;mm&#x2f;yyyy&#x27;&#x29; AND to_date&#x28;stDtfim,&#x27;dd&#x2f;mm&#x2f;yyyy&#x27;&#x29;&#xd;&#xa;						&#xd;&#xa;					union&#xd;&#xa;						select&#xd;&#xa;								pa.cod_conta,&#xd;&#xa;								pa.cod_plano,&#xd;&#xa;								pa.exercicio,&#xd;&#xa;								pa.natureza_saldo,&#xd;&#xa;								pc.atributo_tcemg,&#xd;&#xa;								pc.atributo_tcepe,&#xd;&#xa;								pc.cod_classificacao,&#xd;&#xa;								pc.cod_estrutural,&#xd;&#xa;								pc.cod_sistema,&#xd;&#xa;								pc.escrituracao,&#xd;&#xa;								pc.escrituracao_pcasp,&#xd;&#xa;								pc.funcao,&#xd;&#xa;								pc.indicador_superavit,&#xd;&#xa;								pc.nom_conta,&#xd;&#xa;								pc.obrigatorio_tcmgo,&#xd;&#xa;								cc.cod_entidade,&#xd;&#xa;								cc.cod_lote,&#xd;&#xa;								cc.sequencia,&#xd;&#xa;								cc.tipo,&#xd;&#xa;								cc.tipo_valor,&#xd;&#xa;								vl.oid_lancamento,&#xd;&#xa;								vl.vl_lancamento,&#xd;&#xa;								la.complemento,&#xd;&#xa;								la.cod_historico,&#xd;&#xa;								chc.historico_interno,&#xd;&#xa;								chc.nom_historico,&#xd;&#xa;								le.estorno,&#xd;&#xa;								cl.cod_nota,&#xd;&#xa;								cl.exercicio_liquidacao,&#xd;&#xa;								enl.cod_empenho,&#xd;&#xa;								enl.dt_liquidacao,&#xd;&#xa;								enl.dt_vencimento,&#xd;&#xa;								enl.exercicio_empenho,&#xd;&#xa;								enl.hora,&#xd;&#xa;								enl.observacao,&#xd;&#xa;								ee.cod_categoria,&#xd;&#xa;								ee.cod_pre_empenho,&#xd;&#xa;								ee.dt_empenho,&#xd;&#xa;								ee.restos_pagar,&#xd;&#xa;								ee.vl_saldo_anterior, &#xd;&#xa;								&#x27;C&#x27; as tipo_conta1&#xd;&#xa;						FROM&#xd;&#xa;						             contabilidade.plano_conta       as pc&#xd;&#xa;						            ,contabilidade.plano_analitica   as pa&#xd;&#xa;						            ,contabilidade.conta_credito     as cc&#xd;&#xa;						            ,contabilidade.valor_lancamento  as vl&#xd;&#xa;						            ,contabilidade.lancamento        as la&#xd;&#xa;						LEFT JOIN contabilidade.historico_contabil as chc&#xd;&#xa;						  ON chc.exercicio &#x3d; la.exercicio&#xd;&#xa;						 AND chc.cod_historico &#x3d; la.cod_historico&#xd;&#xa;						LEFT JOIN contabilidade.lancamento_empenho     as le&#xd;&#xa;						  ON le.exercicio &#x3d; la.exercicio&#xd;&#xa;						 AND le.cod_entidade &#x3d; la.cod_entidade&#xd;&#xa;						 AND le.tipo &#x3d; la.tipo&#xd;&#xa;						 AND le.cod_lote &#x3d; la.cod_lote&#xd;&#xa;						 AND le.sequencia &#x3d; la.sequencia&#xd;&#xa;						LEFT JOIN contabilidade.liquidacao as cl&#xd;&#xa;						  ON le.exercicio &#x3d; cl.exercicio&#xd;&#xa;						         AND le.cod_lote &#x3d; cl.cod_lote&#xd;&#xa;						 AND le.tipo &#x3d; cl.tipo&#xd;&#xa;						 AND le.sequencia &#x3d; cl.sequencia&#xd;&#xa;						 AND le.cod_entidade &#x3d; cl.cod_entidade&#xd;&#xa;						LEFT JOIN empenho.nota_liquidacao as enl&#xd;&#xa;						  ON cl.exercicio &#x3d; enl.exercicio&#xd;&#xa;						 AND cl.cod_entidade &#x3d; enl.cod_entidade&#xd;&#xa;						 AND cl.cod_nota &#x3d; enl.cod_nota&#xd;&#xa;						LEFT JOIN empenho.empenho as ee&#xd;&#xa;						  ON ee.exercicio &#x3d; enl.exercicio&#xd;&#xa;						 AND ee.cod_entidade &#x3d; enl.cod_entidade&#xd;&#xa;						 AND ee.cod_empenho &#x3d; enl.cod_empenho&#xd;&#xa;						            ,contabilidade.lote              as lo&#xd;&#xa;						            ,contabilidade.sistema_contabil  as sc&#xd;&#xa;						        WHERE   pc.cod_conta    &#x3d; pa.cod_conta&#xd;&#xa;						        AND     pc.exercicio    &#x3d; pa.exercicio&#xd;&#xa;						        AND     pa.cod_plano    &#x3d; cc.cod_plano&#xd;&#xa;						        AND     pa.exercicio    &#x3d; cc.exercicio&#xd;&#xa;						        AND     cc.cod_lote     &#x3d; vl.cod_lote&#xd;&#xa;						        AND     cc.tipo         &#x3d; vl.tipo&#xd;&#xa;						        AND     cc.sequencia    &#x3d; vl.sequencia&#xd;&#xa;						        AND     cc.exercicio    &#x3d; vl.exercicio&#xd;&#xa;						        AND     cc.tipo_valor   &#x3d; vl.tipo_valor&#xd;&#xa;						        AND     cc.cod_entidade &#x3d; vl.cod_entidade&#xd;&#xa;						        AND     vl.cod_lote     &#x3d; la.cod_lote&#xd;&#xa;						        AND     vl.tipo         &#x3d; la.tipo&#xd;&#xa;						        AND     vl.sequencia    &#x3d; la.sequencia&#xd;&#xa;						        AND     vl.exercicio    &#x3d; la.exercicio&#xd;&#xa;						        AND     vl.cod_entidade &#x3d; la.cod_entidade&#xd;&#xa;						        AND     la.cod_lote     &#x3d; lo.cod_lote&#xd;&#xa;						        AND     la.exercicio    &#x3d; lo.exercicio&#xd;&#xa;						        AND     la.tipo         &#x3d; lo.tipo&#xd;&#xa;						        AND     la.cod_entidade &#x3d; lo.cod_entidade&#xd;&#xa;						        AND     pa.exercicio &#x3d;  stExercicio &#xd;&#xa;						        AND     sc.cod_sistema  &#x3d; pc.cod_sistema&#xd;&#xa;						        AND     sc.exercicio    &#x3d; pc.exercicio&#xd;&#xa;						        AND     lo.dt_lote between to_date&#x28;stDtinicio,&#x27;dd&#x2f;mm&#x2f;yyyy&#x27;&#x29; AND to_date&#x28;stDtfim,&#x27;dd&#x2f;mm&#x2f;yyyy&#x27;&#x29;&#xd;&#xa;						&#xd;&#xa;				&#x29; as tbl&#xd;&#xa;			&#xd;&#xa;			&#x29;&#xd;&#xa;			, TCE_4111_format as &#xd;&#xa;			&#x28;&#xd;&#xa;				  select &#xd;&#xa;					   &#x28;stCnpjSetor    &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtinicio, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29; &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtfim, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;  to_char&#x28;now&#x28;&#x29;, &#x27;ddmmyyyy&#x27;&#x29;					   &#xd;&#xa;					&#x7c;&#x7c;  rpad&#x28; upper&#x28;stNomeSetor&#x29;, 80, &#x27; &#x27;&#x29; &#x29;  as registro_TCE_4111					&#xd;&#xa;				  union all&#xd;&#xa;				  select&#xd;&#xa;				     lpad&#x28;coalesce&#x28;cod_conta&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 20, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_lote&#x3a;&#x3a;varchar, &#x27; &#x27;&#x29;, 28, &#x27;0&#x27;&#x29;				     &#xd;&#xa;			  	  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;sequencia&#x3a;&#x3a;varchar, &#x27; &#x27;&#x29; , 14, &#x27;0&#x27;&#x29;&#xd;&#xa;			  	  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_sistema&#x3a;&#x3a;varchar, &#x27; &#x27;&#x29; , 3, &#x27;0&#x27;&#x29;&#xd;&#xa;			  	  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;exercicio&#x3a;&#x3a;varchar, &#x27; &#x27;&#x29; , 4, &#x27;0&#x27;&#x29;&#xd;&#xa;		  	  	  &#x7c;&#x7c; case &#xd;&#xa;					 	when vl_lancamento&#x3a;&#x3a;numeric &#x3e; 0 then lpad&#x28;        replace&#x28;  coalesce&#x28; vl_lancamento&#x3a;&#x3a;varchar,&#x27;&#x27;&#x29;,  &#x27;.&#x27;,&#x27;&#x27;&#x29;,      17,&#x27;0&#x27;&#x29; &#xd;&#xa;					 	else &#x27;-&#x27; &#x7c;&#x7c; lpad&#x28;replace&#x28;coalesce&#x28;&#x28;vl_lancamento&#x3a;&#x3a;numeric &#x2a; -1&#x29;&#x3a;&#x3a;varchar,&#x27;&#x27;&#x29;,&#x27;.&#x27;,&#x27;&#x27;&#x29;,16,&#x27;0&#x27;&#x29; end&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_historico&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 14, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; rpad&#x28;coalesce&#x28;nom_historico&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 150, &#x27; &#x27;&#x29;&#xd;&#xa;				  as registro_TCE_4111&#xd;&#xa;				  from TCE_4111&#xd;&#xa;			      union all&#xd;&#xa;			      select &#x27;FINALIZADOR&#x27;  &#x7c;&#x7c; lpad&#x28;&#x28;&#x28;select COUNT&#x28;&#x2a;&#x29;&#x3a;&#x3a;varchar  from TCE_4111&#x29;&#x3a;&#x3a;varchar&#x29;, 10, &#x27;0&#x27;&#x29;  as registro_TCE_4111&#xd;&#xa;			&#x29;&#xd;&#xa;				  insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;				  select  &#x27;TCE_4111.TXT&#x27;, json_agg&#x28;     regexp_replace&#x28;  registro_TCE_4111  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;     &#x29; from TCE_4111_format&#x3b;   &#xd;&#xa;    end if&#x3b;		&#xd;&#xa;			&#xd;&#xa;&#xd;&#xa;    if jbArquivos &#x40;&#x3e; &#x27;&#x22;TCE_4960.TXT&#x22;&#x27;&#x3a;&#x3a;jsonb&#xd;&#xa;	 	then  &#xd;&#xa;	 		&#xd;&#xa;			&#xd;&#xa;			with TCE_4960 as&#xd;&#xa;			&#x28;&#xd;&#xa;				SELECT&#xd;&#xa;							&#x27;01&#x2f;01&#x2f;2010&#x27;&#x3a;&#x3a;varchar as dt_inicial_1&#xd;&#xa;							,SUBSTR&#x28;folhapagamento.evento.codigo,2&#x29; as codigo_1&#xd;&#xa;							,folhapagamento.evento.descricao as descricao_1&#xd;&#xa;							,&#x27;N&#xc3;O INFORMADO&#x27;&#x3a;&#x3a;varchar as base_1&#xd;&#xa;				FROM 		folhapagamento.evento&#xd;&#xa;				INNER join	folhapagamento.evento_evento&#xd;&#xa;				        ON  folhapagamento.evento.cod_evento &#x3d; folhapagamento.evento_evento.cod_evento&#xd;&#xa;				GROUP BY&#xd;&#xa;							folhapagamento.evento.codigo,&#xd;&#xa;							folhapagamento.evento.descricao,&#xd;&#xa;							folhapagamento.evento.cod_evento&#xd;&#xa;				 ORDER BY 	folhapagamento.evento.codigo			&#xd;&#xa;			&#x29;&#xd;&#xa;			, TCE_4960_format as &#xd;&#xa;			&#x28;&#xd;&#xa;				  select &#xd;&#xa;					   &#x28;stCnpjSetor    &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtinicio, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29; &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtfim, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;  to_char&#x28;now&#x28;&#x29;, &#x27;ddmmyyyy&#x27;&#x29;					   &#xd;&#xa;					&#x7c;&#x7c;  rpad&#x28; upper&#x28;stNomeSetor&#x29;, 80, &#x27; &#x27;&#x29; &#x29;  as registro_TCE_4960					&#xd;&#xa;				  union all&#xd;&#xa;				  select&#xd;&#xa;				     replace&#x28;dt_inicial_1, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;			  	  &#x7c;&#x7c; &#x27;000&#x27;	&#xd;&#xa;			  	  &#x7c;&#x7c; rpad&#x28;descricao_1, 45, &#x27; &#x27;&#x29;					        &#xd;&#xa;				  &#x7c;&#x7c; base_1&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;&#x27;&#x27;, 137, &#x27; &#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;codigo_1, 4, &#x27;0&#x27;&#x29;&#xd;&#xa;				  as registro_TCE_4960&#xd;&#xa;				  from TCE_4960&#xd;&#xa;			      union all&#xd;&#xa;			      select &#x27;FINALIZADOR&#x27;  &#x7c;&#x7c; lpad&#x28;&#x28;&#x28;select COUNT&#x28;&#x2a;&#x29;&#x3a;&#x3a;varchar  from TCE_4960&#x29;&#x3a;&#x3a;varchar&#x29;, 10, &#x27;0&#x27;&#x29;  as registro_TCE_4960&#xd;&#xa;			&#x29;&#xd;&#xa;				  insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;				  select  &#x27;TCE_4960.TXT&#x27;, json_agg&#x28;     regexp_replace&#x28;  registro_TCE_4960  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;     &