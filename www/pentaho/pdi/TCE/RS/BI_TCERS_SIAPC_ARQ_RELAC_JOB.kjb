    <entry>
      <name>SJ_010_ESS_CREATE_SCHEMA_FUNCTION</name>
      <description/>
      <type>SQL</type>
      <sql>SET search_path &#x3d; &#x27;prestacao_contas&#x27;, pg_catalog&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;CREATE OR REPLACE FUNCTION prestacao_contas.tcers_siapc_arq_relac&#x28;bigint&#x29;&#xd;&#xa; RETURNS text&#xd;&#xa; LANGUAGE plpgsql&#xd;&#xa;AS &#x24;function&#x24;&#xd;&#xa;DECLARE&#xd;&#xa;    inId            ALIAS FOR &#x24;1 &#x3b;&#xd;&#xa;    stParametros 	VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stEntidade		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stExercicio		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stMes			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	jbArquivos 		JSONB        &#x3b;&#xd;&#xa;	stSql     		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stSql2 			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stDtinicio		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stDtfim			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stCnpjSetor		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stNomeSetor		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stOrgao			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	reRegistro      RECORD       &#x3b;	&#xd;&#xa; 	stResultado		VARCHAR &#x3a;&#x3d; &#x27;ok&#x27;&#x3b;&#xd;&#xa;	stError			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b; 	&#xd;&#xa;begin&#xd;&#xa;&#xd;&#xa;	&#xd;&#xa;	select &#xd;&#xa;		valor &#xd;&#xa;	into stParametros &#xd;&#xa;	from prestacao_contas.fila_relatorio &#xd;&#xa;	where id &#x3d; inId&#x3b;&#xd;&#xa;	&#xd;&#xa;	stSql &#x3a;&#x3d; &#x27;select &#x2a; from json_to_record&#x28;&#x27;&#x27;&#x27;&#x7c;&#x7c;stParametros&#x7c;&#x7c;&#x27;	&#x27;&#x27;&#x29;  as x&#x28;entidade text, mes int, exercicio text, arquivo text, data_inicio text, data_fim text, cnpjsetor text, st_cnpj_setor text, orgao text &#x29;&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;	&#xd;&#xa;	FOR reRegistro IN EXECUTE stSql&#xd;&#xa;    LOOP		&#xd;&#xa;		stEntidade	&#x3a;&#x3d; reRegistro.entidade&#x3b;&#xd;&#xa;		stExercicio	&#x3a;&#x3d; reRegistro.exercicio&#x3b;&#xd;&#xa;		stMes		&#x3a;&#x3d; reRegistro.mes&#x3b;&#xd;&#xa;		jbArquivos 	&#x3a;&#x3d; &#x28;upper&#x28;reRegistro.arquivo&#x29;&#x29;&#x3a;&#x3a;jsonb&#x3b;&#xd;&#xa;		stDtinicio	&#x3a;&#x3d; reRegistro.data_inicio&#x3b;		&#xd;&#xa;		stDtfim		&#x3a;&#x3d; reRegistro.data_fim&#x3b;&#xd;&#xa;		stCnpjSetor	&#x3a;&#x3d; reRegistro.cnpjSetor&#x3b;&#xd;&#xa;		stNomeSetor	&#x3a;&#x3d; reRegistro.st_cnpj_setor&#x3b;&#xd;&#xa;		stOrgao		&#x3a;&#x3d; reRegistro.orgao&#x3b;&#xd;&#xa;    END LOOP&#x3b;&#xd;&#xa;    &#xd;&#xa;&#xd;&#xa;    CREATE TEMPORARY TABLE tmp_table &#x28;arquivo varchar&#x28;200&#x29;, content json&#x29;&#x3b;&#xd;&#xa;    &#xd;&#xa;    &#xd;&#xa;    &#xd;&#xa;    if jbArquivos &#x40;&#x3e; &#x27;&#x22;CTA_DISP.TXT&#x22;&#x27;&#x3a;&#x3a;jsonb&#xd;&#xa;	 	then  &#xd;&#xa;	 		&#xd;&#xa;			&#xd;&#xa;			with ctadisp as&#xd;&#xa;			&#x28;&#xd;&#xa;				SELECT &#x2a;,&#xd;&#xa;				         CASE WHEN cod_entidade &#x3d; cod_entidade_prefeitura&#x3a;&#x3a;INTEGER THEN 1&#xd;&#xa;				              WHEN cod_entidade &#x3d; cod_entidade_camara&#x3a;&#x3a;INTEGER THEN 2&#xd;&#xa;				              WHEN cod_entidade &#x3d; cod_entidade_rpps&#x3a;&#x3a;INTEGER THEN 3&#xd;&#xa;				              ELSE 9&#xd;&#xa;				         END AS cod_classificacao&#xd;&#xa;				        FROM &#x28;&#xd;&#xa;				 SELECT&#xd;&#xa;				     &#x28;SELECT valor FROM administracao.configuracao WHERE parametro &#x3d; &#x27;cod_entidade_prefeitura&#x27; and exercicio &#x3d; &#x27;2012&#x27;&#x29; AS cod_entidade_prefeitura ,&#xd;&#xa;				     &#x28;SELECT valor FROM administracao.configuracao WHERE parametro &#x3d; &#x27;cod_entidade_camara&#x27; and exercicio &#x3d; &#x27;2012&#x27;&#x29; AS cod_entidade_camara ,&#xd;&#xa;				     &#x28;SELECT valor FROM administracao.configuracao WHERE parametro &#x3d; &#x27;cod_entidade_rpps&#x27; and exercicio &#x3d; &#x27;2012&#x27;&#x29; AS cod_entidade_rpps ,&#xd;&#xa;				     replace&#x28;pc.cod_estrutural,&#x27;.&#x27;,&#x27;&#x27;&#x29; as cod_estrutural,&#xd;&#xa;				     &#x27;9999&#x27; as orgao_unidade,&#xd;&#xa;				     case when pce.cod_entidade is not null then pce.cod_entidade else pb.cod_entidade end as cod_entidade,&#xd;&#xa;				     pr.cod_recurso,&#xd;&#xa;				     replace&#x28;pb.cod_banco&#x3a;&#x3a;VARCHAR,&#x27;-&#x27;,&#x27;&#x27;&#x29; as cod_banco,&#xd;&#xa;				     replace&#x28;replace&#x28;replace&#x28;pb.cod_agencia&#x3a;&#x3a;VARCHAR,&#x27; &#x27;,&#x27;&#x27;&#x29;,&#x27;.&#x27;,&#x27;&#x27;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; as cod_agencia,&#xd;&#xa;				     replace&#x28;replace&#x28;replace&#x28;pb.conta_corrente,&#x27; &#x27;,&#x27;&#x27;&#x29;,&#x27;.&#x27;,&#x27;&#x27;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; as conta_corrente,&#xd;&#xa;				     CASE SUBSTR&#x28;REPLACE&#x28;pc.cod_estrutural,&#x27;.&#x27;,&#x27;&#x27;&#x29;,1,7&#x29;&#xd;&#xa;				        WHEN &#x27;1111101&#x27; THEN &#x27;1&#x27;&#xd;&#xa;				        WHEN &#x27;1111119&#x27; THEN &#x27;2&#x27;&#xd;&#xa;				        WHEN &#x27;1111106&#x27; THEN &#x27;2&#x27;&#xd;&#xa;				        WHEN &#x27;1111150&#x27; THEN &#x27;3&#x27;&#xd;&#xa;				        WHEN &#x27;114&#x25;&#x27;  THEN &#x27;3&#x27;&#xd;&#xa;				    END as tipo_conta&#xd;&#xa;				 FROM&#xd;&#xa;				     contabilidade.plano_conta as pc&#xd;&#xa;				     JOIN contabilidade.classificacao_contabil as cc ON&#xd;&#xa;				         cc.exercicio &#x3d; pc.exercicio AND&#xd;&#xa;				         cc.cod_classificacao &#x3d; pc.cod_classificacao&#xd;&#xa;				      LEFT JOIN  tcers.plano_conta_entidade as pce&#xd;&#xa;				          ON &#x28;   pc.cod_conta   &#x3d; pce.cod_conta AND&#xd;&#xa;				                 pc.exercicio   &#x3d; pce.exercicio &#x29;&#xd;&#xa;				     JOIN contabilidade.plano_analitica as pa ON&#xd;&#xa;				         pa.cod_conta &#x3d; pc.cod_conta AND&#xd;&#xa;				         pa.exercicio &#x3d; pc.exercicio&#xd;&#xa;				     LEFT JOIN contabilidade.plano_banco as pb ON&#xd;&#xa;				         pb.cod_plano &#x3d; pa.cod_plano AND&#xd;&#xa;				         pb.exercicio &#x3d; pa.exercicio AND&#xd;&#xa;				         pb.cod_entidade in  &#x28;   select  unnest&#x28;regexp_split_to_array&#x28;stEntidade, E&#x27;&#x5c;,&#x27;&#x29;&#x29;&#x3a;&#x3a;int   &#x29; &#xd;&#xa;				     LEFT JOIN contabilidade.plano_recurso as pr ON&#xd;&#xa;				         pr.cod_plano &#x3d; pa.cod_plano AND&#xd;&#xa;				         pr.exercicio &#x3d; pa.exercicio&#xd;&#xa;				 WHERE  cc.exercicio &#x3d;   stExercicio  &#xd;&#xa;				   AND &#x28;SUBSTR&#x28;REPLACE&#x28;pc.cod_estrutural,&#x27;.&#x27;,&#x27;&#x27;&#x29;,1,3&#x29; &#x3d; &#x27;111&#x27;&#x29;&#xd;&#xa;				   AND &#x28;SUBSTR&#x28;REPLACE&#x28;pc.cod_estrutural,&#x27;.&#x27;,&#x27;&#x27;&#x29;,1,3&#x29; &#x3c;&#x3e; &#x27;115&#x27;&#x29;&#xd;&#xa;				   AND pc.cod_sistema &#x3d; 1&#xd;&#xa;				 and &#x28;&#xd;&#xa;				   pa.exercicio&#x7c;&#x7c;pa.cod_plano in&#x28;&#xd;&#xa;				                 select cd.exercicio&#x7c;&#x7c;cd.cod_plano&#xd;&#xa;				                  from  contabilidade.conta_debito      as cd&#xd;&#xa;				                   join contabilidade.valor_lancamento  as vl on&#xd;&#xa;				                   &#x28;&#xd;&#xa;				                     cd.cod_lote          &#x3d; vl.cod_lote&#xd;&#xa;				                     and cd.tipo          &#x3d; vl.tipo&#xd;&#xa;				                     and cd.sequencia     &#x3d; vl.sequencia&#xd;&#xa;				                     and cd.exercicio     &#x3d; vl.exercicio&#xd;&#xa;				                     and cd.tipo_valor    &#x3d; vl.tipo_valor&#xd;&#xa;				                     and cd.cod_entidade  &#x3d; vl.cod_entidade&#xd;&#xa;				                   &#x29;&#xd;&#xa;				                   join contabilidade.lancamento  as la on&#xd;&#xa;				                   &#x28;&#xd;&#xa;				                     la.sequencia         &#x3d; vl.sequencia&#xd;&#xa;				                     and la.cod_lote      &#x3d; vl.cod_lote&#xd;&#xa;				                     and la.tipo          &#x3d; vl.tipo&#xd;&#xa;				                     and la.exercicio     &#x3d; vl.exercicio&#xd;&#xa;				                     and la.cod_entidade  &#x3d; vl.cod_entidade&#xd;&#xa;				                   &#x29;&#xd;&#xa;				                   join contabilidade.lote as lo on&#xd;&#xa;				                   &#x28;&#xd;&#xa;				                     lo.cod_lote          &#x3d; la.cod_lote&#xd;&#xa;				                     and lo.exercicio     &#x3d; la.exercicio&#xd;&#xa;				                     and lo.tipo          &#x3d; la.tipo&#xd;&#xa;				                     and lo.cod_entidade  &#x3d; la.cod_entidade&#xd;&#xa;				                   &#x29;&#xd;&#xa;				                  where&#xd;&#xa;				                   vl.vl_lancamento &#x3c;&#x3e; 0.00&#xd;&#xa;				                   and lo.dt_lote between&#xd;&#xa;				                             to_date&#x28;stDtinicio,&#x27;dd&#x2f;mm&#x2f;yyyy&#x27;&#x29; AND&#xd;&#xa;				                             to_date&#x28;stDtfim,&#x27;dd&#x2f;mm&#x2f;yyyy&#x27;&#x29;&#xd;&#xa;				                    and cd.cod_entidade IN 	&#x28;select  unnest&#x28;regexp_split_to_array&#x28;stEntidade, E&#x27;&#x5c;,&#x27;&#x29;&#x29;&#x3a;&#x3a;int &#x29; &#xd;&#xa;				                   group by cd.exercicio, cd.cod_plano&#xd;&#xa;				               &#x29;&#xd;&#xa;				   or&#xd;&#xa;				   pa.exercicio&#x7c;&#x7c;pa.cod_plano in&#x28;&#xd;&#xa;				                 select cc.exercicio&#x7c;&#x7c;cc.cod_plano&#xd;&#xa;				                  from  contabilidade.conta_credito     as cc&#xd;&#xa;				                   join contabilidade.valor_lancamento  as vl on&#xd;&#xa;				                   &#x28;&#xd;&#xa;				                     cc.cod_lote          &#x3d; vl.cod_lote&#xd;&#xa;				                     and cc.tipo          &#x3d; vl.tipo&#xd;&#xa;				                     and cc.sequencia     &#x3d; vl.sequencia&#xd;&#xa;				                     and cc.exercicio     &#x3d; vl.exercicio&#xd;&#xa;				                     and cc.tipo_valor    &#x3d; vl.tipo_valor&#xd;&#xa;				                     and cc.cod_entidade  &#x3d; vl.cod_entidade&#xd;&#xa;				                   &#x29;&#xd;&#xa;				                  join contabilidade.lancamento  as la on&#xd;&#xa;				                   &#x28;&#xd;&#xa;				                     la.sequencia         &#x3d; vl.sequencia&#xd;&#xa;				                     and la.cod_lote      &#x3d; vl.cod_lote&#xd;&#xa;				                     and la.tipo          &#x3d; vl.tipo&#xd;&#xa;				                     and la.exercicio     &#x3d; vl.exercicio&#xd;&#xa;				                     and la.cod_entidade  &#x3d; vl.cod_entidade&#xd;&#xa;				                   &#x29;&#xd;&#xa;				                   join contabilidade.lote as lo on&#xd;&#xa;				                   &#x28;&#xd;&#xa;				                     lo.cod_lote          &#x3d; la.cod_lote&#xd;&#xa;				                     and lo.exercicio     &#x3d; la.exercicio&#xd;&#xa;				                     and lo.tipo          &#x3d; la.tipo&#xd;&#xa;				                     and lo.cod_entidade  &#x3d; la.cod_entidade&#xd;&#xa;				                   &#x29;&#xd;&#xa;				                  where&#xd;&#xa;				                   vl.vl_lancamento &#x3c;&#x3e; 0.00&#xd;&#xa;				                   and lo.dt_lote between&#xd;&#xa;				                             &#xd;&#xa;				                             to_date&#x28;stDtinicio,&#x27;dd&#x2f;mm&#x2f;yyyy&#x27;&#x29; AND	      to_date&#x28;stDtfim,&#x27;dd&#x2f;mm&#x2f;yyyy&#x27;&#x29;&#xd;&#xa;				                    and cc.cod_entidade IN  &#x28;select  unnest&#x28;regexp_split_to_array&#x28;stEntidade, E&#x27;&#x5c;,&#x27;&#x29;&#x29;&#x3a;&#x3a;int	&#x29;&#xd;&#xa;				                   group by cc.exercicio, cc.cod_plano&#xd;&#xa;				                 &#x29;&#xd;&#xa;				  &#x29; order by cod_estrutural&#xd;&#xa;				  &#x29; AS tabela&#xd;&#xa;			&#x29;&#xd;&#xa;			, ctadisp_format as &#xd;&#xa;			&#x28;&#xd;&#xa;				  select &#xd;&#xa;					   &#x28;stCnpjSetor    &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtinicio, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29; &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtfim, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;  to_char&#x28;now&#x28;&#x29;, &#x27;ddmmyyyy&#x27;&#x29;					   &#xd;&#xa;					&#x7c;&#x7c;  rpad&#x28; upper&#x28;stNomeSetor&#x29;, 80, &#x27; &#x27;&#x29; &#x29;  as registro_ctadisp					&#xd;&#xa;				  union all&#xd;&#xa;				  select&#xd;&#xa;				     lpad&#x28;coalesce&#x28;cod_estrutural&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 20, &#x27;0&#x27;&#x29;&#xd;&#xa;			  	  &#x7c;&#x7c; lpad&#x28;stOrgao, 4, &#x27;0&#x27;&#x29;		  &#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_recurso&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 4, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_banco&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 5, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_agencia&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 5, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;conta_corrente&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 20, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;tipo_conta&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 1, &#x27;0&#x27;&#x29;&#xd;&#xa;				  &#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_classificacao&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;, 1, &#x27;0&#x27;&#x29;&#xd;&#xa;				  as registro_ctadisp&#xd;&#xa;				  from ctadisp&#xd;&#xa;			      union all&#xd;&#xa;			      select &#x27;FINALIZADOR&#x27;  &#x7c;&#x7c; lpad&#x28;&#x28;&#x28;select COUNT&#x28;&#x2a;&#x29;&#x3a;&#x3a;varchar  from ctadisp&#x29;&#x3a;&#x3a;varchar&#x29;, 10, &#x27;0&#x27;&#x29;  as registro_ctadisp&#xd;&#xa;			&#x29;&#xd;&#xa;				  insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;				  select  &#x27;CTA_DISP.TXT&#x27;, json_agg&#x28;     regexp_replace&#x28;  registro_ctadisp  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;     &#x29; from ctadisp_format&#x3b;   &#xd;&#xa;    end if&#x3b;		&#xd;&#xa;&#xd;&#xa;    if jbArquivos &#x40;&#x3e; &#x27;&#x22;CTA_OPER.TXT&#x22;&#x27;&#x3a;&#x3a;jsonb&#xd;&#xa;	 	then      &#xd;&#xa;	 		&#xd;&#xa;			&#xd;&#xa;			with ctaoper as&#xd;&#xa;			&#x28;&#xd;&#xa;				SELECT&#xd;&#xa;				    cod_modulo ,&#xd;&#xa;				    parametro ,&#xd;&#xa;				    valor ,&#xd;&#xa;				    exercicio&#xd;&#xa;				FROM&#xd;&#xa;				    administracao.configuracao WHERE parametro &#x3d; &#x27;gc_arq&#x27;&#xd;&#xa;			&#x29;&#xd;&#xa;			, ctaoper_format as &#xd;&#xa;			&#x28;&#xd;&#xa;			      &#xd;&#xa;				  select &#xd;&#xa;					   &#x28;stCnpjSetor    &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtinicio, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29; &#xd;&#xa;					&#x7c;&#x7c;  replace&#x28;stDtfim, &#x27;&#x2f;&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;  to_char&#x28;now&#x28;&#x29;, &#x27;ddmmyyyy&#x27;&#x29;					   &#xd;&#xa;					&#x7c;&#x7c;  rpad&#x28; upper&#x28;stNomeSetor&#x29;, 80, &#x27; &#x27;&#x29; &#x29;  as registro_ctaoper					&#xd;&#xa;				  union all&#xd;&#xa;			      select &#x27;FINALIZADOR&#x27;  &#x7c;&#x7c; lpad&#x28;&#x28;&#x28;select COUNT&#x28;&#x2a;&#x29;&#x3a;&#x3a;varchar  from ctaoper&#x29;&#x3a;&#x3a;varchar&#x29;, 10, &#x27;0&#x27;&#x29;  as registro_ctaoper&#xd;&#xa;			&#x29;&#xd;&#xa;				  insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;				  select  &#x27;CTA_OPER.TXT&#x27;, json_agg&#x28;     regexp_replace&#x28;  registro_ctaoper  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;     &#x29; from ctaoper_format&#x3b;   &#xd;&#xa;    end if&#x3b;		&#xd;&#xa;			&#xd;&#xa;    &#xd;&#xa;    update prestacao_contas.fila_relatorio &#xd;&#xa;    	set &#xd;&#xa;    		  resposta &#x3d; &#x28;select  json_object_agg&#x28;t.arquivo,t.content&#x29; from tmp_table t&#x29;&#xd;&#xa;      	where id &#x3d; inId&#x3b;&#xd;&#xa;    &#xd;&#xa;    &#xd;&#xa;    DROP TABLE tmp_table&#x3b;&#xd;&#xa;&#xd;&#xa;    return stResultado&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa; EXCEPTION  &#xd;&#xa;   	when others then&#xd;&#xa;   	    &#xd;&#xa;	    stError &#x3a;&#x3d; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;SQLErro&#x27;, REPLACE&#x28; SQLERRM, &#x27;&#x22;&#x27;, &#x27;&#x2a;&#x27;&#x29;&#x29;&#x3b;&#xd;&#xa;	    stError &#x3a;&#x3d; &#x27;&#x27; &#x7c;&#x7c; stError &#x7c;&#x7c; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;Entidade&#x27;, COALESCE&#x28;stEntidade&#x3a;&#x3a;varchar,&#x27;&#x27;&#x29;&#x29;&#x3b;&#xd;&#xa;	    stError &#x3a;&#x3d; &#x27;&#x27; &#x7c;&#x7c; stError &#x7c;&#x7c; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;Exercicio&#x27;, COALESCE&#x28;stExercicio&#x3a;&#x3a;varchar,&#x27;&#x27;&#x29;&#x29;&#x3b;&#xd;&#xa;	    --stError &#x3a;&#x3d; &#x27;&#x27; &#x7c;&#x7c; stError &#x7c;&#x7c; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;Mes&#x27;, COALESCE&#x28;CAST&#x28;stMes as text&#x29;, &#x27;&#x27;&#x29;&#x29;&#x3b;&#xd;&#xa;	    stError &#x3a;&#x3d; &#x27;&#x27; &#x7c;&#x7c; stError &#x7c;&#x7c; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;Mes&#x27;, &#x27;N_UTIL&#x27;&#x29;&#x3b;&#xd;&#xa;	    stError &#x3a;&#x3d; &#x27;&#x27; &#x7c;&#x7c; stError &#x7c;&#x7c; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;Dtinicio&#x27;, COALESCE&#x28;stDtinicio&#x3a;&#x3a;varchar,&#x27;&#x27;&#x29;&#x29;&#x3b;&#xd;&#xa;	    stError &#x3a;&#x3d; &#x27;&#x27; &#x7c;&#x7c; stError &#x7c;&#x7c; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;Dtfim&#x27;, COALESCE&#x28;stDtfim&#x3a;&#x3a;varchar,&#x27;&#x27;&#x29;&#x29;&#x3b;&#xd;&#xa;	    stError &#x3a;&#x3d; &#x27;&#x27; &#x7c;&#x7c; stError &#x7c;&#x7c; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;Orgao&#x27;, COALESCE&#x28;stOrgao&#x3a;&#x3a;varchar,&#x27;&#x27;&#x29;&#x29;&#x3b;&#xd;&#xa;    &#xd;&#xa;   		stResultado &#x3a;&#x3d;  &#x27;nok&#x27;&#x3b;&#xd;&#xa;   		&#xd;&#xa;   		&#xd;&#xa;   		stError &#x3a;&#x3d; prestacao_contas.processa_status_erro&#x28;inId, stError&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;   		Return stResultado&#x3b;	    	&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;END&#x3b;&#xd;&#xa;&#x24;function&#x24;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
