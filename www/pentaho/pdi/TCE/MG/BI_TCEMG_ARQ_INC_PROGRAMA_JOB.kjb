    <entry>
      <name>SJ_010_ESS_CREATE_SCHEMA_FUNCTION</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;SET search_path &#x3d; &#x27;prestacao_contas&#x27;, pg_catalog&#x3b;&#xd;&#xa;&#xd;&#xa;CREATE OR REPLACE FUNCTION prestacao_contas.tcemg_sicom_arq_inclusao_programas&#x28;bigint&#x29;&#xd;&#xa; RETURNS text&#xd;&#xa; LANGUAGE plpgsql&#xd;&#xa;AS &#x24;function&#x24;&#xd;&#xa;DECLARE&#xd;&#xa;    inId ALIAS FOR &#x24;1&#x3b;&#xd;&#xa;    stParametros 	VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stExercicio		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stMes			INT     &#x3a;&#x3d; 0&#x3b;&#xd;&#xa;	stEntidade		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stOrgao			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;	&#xd;&#xa;	stOrgaoIncamp	VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;	&#xd;&#xa;	stDtinicio		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stDtfim			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stArquivos 		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stSql     		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stSql2 			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa; 	reRegistro      RECORD&#x3b;	&#xd;&#xa; 	stResultado		VARCHAR &#x3a;&#x3d; &#x27;ok&#x27;&#x3b;&#xd;&#xa; 	stError			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa; 	reRecPar 		RECORD&#x3b;&#xd;&#xa;begin&#xd;&#xa;	&#xd;&#xa;	select &#xd;&#xa;		valor &#xd;&#xa;	into stParametros &#xd;&#xa;	from prestacao_contas.fila_relatorio &#xd;&#xa;	where id &#x3d; inId&#x3b;&#xd;&#xa;	&#xd;&#xa;	stSql &#x3a;&#x3d; &#x27;select &#x2a; from json_to_record&#x28;&#x27;&#x27;&#x27;&#x7c;&#x7c;stParametros&#x7c;&#x7c;&#x27;	&#x27;&#x27;&#x29;  as x&#x28;exercicio int, mes int, entidade text, orgao text, orgao_incamp text, data_inicio text, data_fim text, arquivo text &#x29;&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;	FOR reRegistro IN EXECUTE stSql&#xd;&#xa;    LOOP		&#xd;&#xa;		stExercicio		&#x3a;&#x3d; reRegistro.exercicio&#x3b;&#xd;&#xa;		stMes			&#x3a;&#x3d; reRegistro.mes&#x3b;&#xd;&#xa;		stEntidade		&#x3a;&#x3d; reRegistro.entidade&#x3b;&#xd;&#xa;		stOrgao			&#x3a;&#x3d; reRegistro.orgao&#x3b;&#xd;&#xa;		stOrgaoIncamp	&#x3a;&#x3d; reRegistro.orgao_incamp&#x3b;		&#xd;&#xa;		stDtinicio		&#x3a;&#x3d; reRegistro.data_inicio&#x3b;		&#xd;&#xa;		stDtfim			&#x3a;&#x3d; reRegistro.data_fim&#x3b;&#xd;&#xa;		stArquivos 		&#x3a;&#x3d; reRegistro.arquivo&#x3b;&#xd;&#xa;    END LOOP&#x3b;&#xd;&#xa;    &#xd;&#xa;    CREATE TEMPORARY TABLE tmp_table &#x28;arquivo varchar&#x28;200&#x29;, content json&#x29;&#x3b;&#xd;&#xa;    &#xd;&#xa;    if strpos&#x28;stArquivos,&#x27;&#x22;IDE.csv&#x22;&#x27;&#x29; &#x3e; 0&#xd;&#xa;	 	then  &#xd;&#xa;			with ide as&#xd;&#xa;			&#x28;&#xd;&#xa;				SELECT &#x28;&#xd;&#xa;					  SELECT cnpj&#xd;&#xa;						FROM sw_cgm_pessoa_juridica&#xd;&#xa;				   LEFT JOIN administracao.configuracao&#xd;&#xa;						  ON parametro &#x3d; &#x27;cod_entidade_prefeitura&#x27;&#xd;&#xa;						 AND configuracao.exercicio &#x3d; stExercicio  &#xd;&#xa;				   LEFT JOIN orcamento.entidade&#xd;&#xa;						  ON cod_entidade&#x3d;configuracao.valor&#x3a;&#x3a;integer&#xd;&#xa;						 AND entidade.exercicio &#x3d; configuracao.exercicio&#xd;&#xa;					   WHERE sw_cgm_pessoa_juridica.numcgm&#x3d;entidade.numcgm&#xd;&#xa;					 &#x29; AS cnpj_municipio&#xd;&#xa;				   , &#x28; SELECT DISTINCT cod_municipio&#xd;&#xa;						 FROM tcemg.configurar_ide&#xd;&#xa;					 &#x29; AS cod_municipio&#xd;&#xa;				   , LPAD&#x28;&#x28;&#xd;&#xa;						   SELECT valor&#xd;&#xa;							 FROM tcemg.orgao&#xd;&#xa;					   INNER JOIN administracao.configuracao_entidade&#xd;&#xa;							   ON configuracao_entidade.valor&#x3a;&#x3a;integer &#x3d; orgao.num_orgao&#xd;&#xa;							WHERE configuracao_entidade.exercicio   &#x3d; ACE.exercicio&#xd;&#xa;							  AND configuracao_entidade.cod_entidade &#x3d; ACE.cod_entidade&#xd;&#xa;							  AND parametro &#x3d; &#x27;tcemg_codigo_orgao_entidade_sicom&#x27;&#xd;&#xa;					 &#x29;, 2, &#x27;0&#x27;&#x29; AS cod_orgao&#xd;&#xa;				   , &#x28;&#xd;&#xa;					  SELECT valor&#xd;&#xa;						FROM tcemg.orgao&#xd;&#xa;				  INNER JOIN administracao.configuracao_entidade&#xd;&#xa;						  ON configuracao_entidade.valor&#x3a;&#x3a;integer &#x3d; orgao.num_orgao&#xd;&#xa;					   WHERE configuracao_entidade.exercicio    &#x3d; ACE.exercicio&#xd;&#xa;						 AND configuracao_entidade.cod_entidade &#x3d; ACE.cod_entidade&#xd;&#xa;						 AND parametro &#x3d; &#x27;tcemg_tipo_orgao_entidade_sicom&#x27;&#xd;&#xa;					 &#x29; AS tipo_orgao&#xd;&#xa;				   &#xd;&#xa;				   , stExercicio AS exercicio_referencia&#xd;&#xa;				   &#xd;&#xa;				   , stMes AS mes_referencia				   &#xd;&#xa;				   , to_char&#x28;CURRENT_DATE&#x3a;&#x3a;timestamp,&#x27;ddmmyyyy&#x27;&#x29; as data_geracao&#xd;&#xa;				   &#xd;&#xa;				   , to_char&#x28;CURRENT_DATE&#x3a;&#x3a;timestamp,&#x27;yyyymmdd&#x27;&#x29;&#x7c;&#x7c;&#x27;&#x27;&#x7c;&#x7c; stExercicio &#x7c;&#x7c; stMes as cod_remessa&#xd;&#xa;				FROM administracao.configuracao_entidade AS ACE&#xd;&#xa;			   WHERE ACE.exercicio &#x3d;  stExercicio &#xd;&#xa;				 AND ACE.cod_entidade IN  &#x28;select  unnest&#x28;regexp_split_to_array&#x28;stEntidade, E&#x27;&#x5c;,&#x27;&#x29;&#x29;&#x3a;&#x3a;int&#x29;   &#xd;&#xa;				 AND ACE.parametro &#x3d; &#x27;tcemg_codigo_orgao_entidade_sicom&#x27;&#xd;&#xa;			&#x29;       &#xd;&#xa;			, ide_format as &#xd;&#xa;			&#x28;&#xd;&#xa;			    select &#xd;&#xa;			                coalesce&#x28;lpad&#x28;case when cod_municipio&#x3a;&#x3a;varchar &#x21;&#x3d; &#x27;&#x27; then cod_municipio&#x3a;&#x3a;varchar else null end,5,&#x27;0&#x27;&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;			        &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;replace&#x28;coalesce&#x28;cnpj_municipio&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;,&#x27;&#x3b;&#x27;,&#x27;&#x7c;&#x27;&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;			        &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;lpad&#x28;case when cod_orgao&#x3a;&#x3a;varchar &#x21;&#x3d; &#x27;&#x27; then cod_orgao&#x3a;&#x3a;varchar else null end,2,&#x27;0&#x27;&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;			        &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;lpad&#x28;case when tipo_orgao&#x3a;&#x3a;varchar &#x21;&#x3d; &#x27;&#x27; then tipo_orgao&#x3a;&#x3a;varchar else null end,2,&#x27;0&#x27;&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;			        &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;exercicio_referencia&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;mes_referencia&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;data_geracao&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;				    &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;replace&#x28;substring&#x28;cod_remessa&#x3a;&#x3a;varchar,1,20&#x29;,&#x27;&#x3b;&#x27;,&#x27;&#x7c;&#x27;&#x29;,&#x27; &#x27;&#x29;				   &#xd;&#xa;			       as registro_ide&#xd;&#xa;			    from ide&#xd;&#xa;			&#x29;&#xd;&#xa;			insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;			select  &#x27;IDE.csv&#x27;, json_agg&#x28;     regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;  registro_ide  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;,&#x27;&#x5e;&#x3b;&#x27;,&#x27; &#x3b;&#x27;&#x29;,&#x27;&#x3b;&#x3b;&#x27;,&#x27;&#x3b; &#x3b;&#x27;,&#x27;g&#x27;&#x29;,&#x27;&#x3b;&#x24;&#x27;,&#x27;&#x3b; &#x27;&#x29;     &#x29; from ide_format&#x3b;   &#xd;&#xa;			&#xd;&#xa;	end if&#x3b;&#xd;&#xa;&#xd;&#xa;    if strpos&#x28;stArquivos,&#x27;&#x22;INCAMP.csv&#x22;&#x27;&#x29; &#x3e; 0&#xd;&#xa;		then&#xd;&#xa;		&#xd;&#xa;			with incamp10 as&#xd;&#xa;			&#x28;&#xd;&#xa;  					  SELECT 10 AS tipo_registro&#xd;&#xa;	                        , 2 AS possui_sub_acao&#xd;&#xa;	                        , LPAD&#x28;acao.num_acao&#x3a;&#x3a;VARCHAR, 4, &#x27;0&#x27;&#x29; AS id_acao&#xd;&#xa;	                        , acao.cod_acao&#xd;&#xa;	                        , acao_dados.descricao AS desc_acao&#xd;&#xa;	                        , acao_dados.finalidade AS finalidade_acao&#xd;&#xa;	                        , produto.descricao AS produto&#xd;&#xa;	                        , unidade_medida.nom_unidade AS unidade_medida&#xd;&#xa;	                        , REPLACE&#x28;SUM&#x28;total_metas.ano1&#x29;&#x3a;&#x3a;VARCHAR, &#x27;.&#x27;,&#x27;,&#x27;&#x29; AS metas_ano_1&#xd;&#xa;	                        , REPLACE&#x28;SUM&#x28;total_metas.ano2&#x29;&#x3a;&#x3a;VARCHAR, &#x27;.&#x27;,&#x27;,&#x27;&#x29; AS metas_ano_2&#xd;&#xa;	                        , REPLACE&#x28;SUM&#x28;total_metas.ano3&#x29;&#x3a;&#x3a;VARCHAR, &#x27;.&#x27;,&#x27;,&#x27;&#x29; AS metas_ano_3&#xd;&#xa;	                        , REPLACE&#x28;SUM&#x28;total_metas.ano4&#x29;&#x3a;&#x3a;VARCHAR, &#x27;.&#x27;,&#x27;,&#x27;&#x29; AS metas_ano_4&#xd;&#xa;	                        , REPLACE&#x28;SUM&#x28;total_recursos.ano1&#x29;&#x3a;&#x3a;VARCHAR, &#x27;.&#x27;,&#x27;,&#x27;&#x29; AS recursos_ano_1&#xd;&#xa;	                        , REPLACE&#x28;SUM&#x28;total_recursos.ano2&#x29;&#x3a;&#x3a;VARCHAR, &#x27;.&#x27;,&#x27;,&#x27;&#x29; AS recursos_ano_2&#xd;&#xa;	                        , REPLACE&#x28;SUM&#x28;total_recursos.ano3&#x29;&#x3a;&#x3a;VARCHAR, &#x27;.&#x27;,&#x27;,&#x27;&#x29; AS recursos_ano_3&#xd;&#xa;	                        , REPLACE&#x28;SUM&#x28;total_recursos.ano4&#x29;&#x3a;&#x3a;VARCHAR, &#x27;.&#x27;,&#x27;,&#x27;&#x29; AS recursos_ano_4&#xd;&#xa;	                        FROM ppa.acao&#xd;&#xa;	                        JOIN ppa.programa&#xd;&#xa;	                          ON acao.cod_programa &#x3d; programa.cod_programa&#xd;&#xa;	                        JOIN ppa.programa_dados&#xd;&#xa;	                          ON programa.cod_programa &#x3d; programa_dados.cod_programa&#xd;&#xa;	                         AND programa.ultimo_timestamp_programa_dados &#x3d; programa_dados.timestamp_programa_dados&#xd;&#xa;	                        JOIN ppa.programa_setorial&#xd;&#xa;	                          ON programa.cod_setorial &#x3d; programa_setorial.cod_setorial&#xd;&#xa;	                        JOIN ppa.macro_objetivo&#xd;&#xa;	                          ON programa_setorial.cod_macro &#x3d; macro_objetivo.cod_macro&#xd;&#xa;	                        JOIN ppa.ppa&#xd;&#xa;	                          ON macro_objetivo.cod_ppa &#x3d; ppa.cod_ppa&#xd;&#xa;	                        JOIN ppa.acao_dados&#xd;&#xa;	                          ON acao.cod_acao &#x3d; acao_dados.cod_acao&#xd;&#xa;	                         AND acao.ultimo_timestamp_acao_dados &#x3d; acao_dados.timestamp_acao_dados&#xd;&#xa;	                   LEFT JOIN ppa.produto&#xd;&#xa;	                          ON acao_dados.cod_produto &#x3d; produto.cod_produto&#xd;&#xa;	                   LEFT JOIN administracao.unidade_medida&#xd;&#xa;	                          ON unidade_medida.cod_unidade  &#x3d; acao_dados.cod_unidade_medida&#xd;&#xa;	                         AND unidade_medida.cod_grandeza &#x3d; acao_dados.cod_grandeza&#xd;&#xa;	                        JOIN &#x28;&#xd;&#xa;	                                SELECT ano1.cod_acao&#xd;&#xa;	                                     , ano1.timestamp_acao_dados&#xd;&#xa;	                                     , ano1.exercicio_recurso&#xd;&#xa;	                                     , COALESCE&#x28;ano1.valor, 0.00&#x29; AS ano1&#xd;&#xa;	                                     , COALESCE&#x28;ano2.valor, 0.00&#x29; AS ano2&#xd;&#xa;	                                     , COALESCE&#x28;ano3.valor, 0.00&#x29; AS ano3&#xd;&#xa;	                                     , COALESCE&#x28;ano4.valor, 0.00&#x29; AS ano4&#xd;&#xa;	                                FROM ppa.acao_recurso AS ano1&#xd;&#xa;	                          &#xd;&#xa;	                          INNER JOIN orcamento.recurso&#x28;stExercicio&#x29; AS recurso  	                          &#xd;&#xa;	                                  ON ano1.cod_recurso   &#x3d; recurso.cod_recurso&#xd;&#xa;	                           LEFT JOIN ppa.acao_recurso AS ano2&#xd;&#xa;	                                  ON ano2.ano &#x3d; &#x27;2&#x27;&#xd;&#xa;	                                 AND ano1.cod_acao             &#x3d; ano2.cod_acao&#xd;&#xa;	                                 AND ano1.timestamp_acao_dados &#x3d; ano2.timestamp_acao_dados&#xd;&#xa;	                                 AND ano1.cod_recurso          &#x3d; ano2.cod_recurso&#xd;&#xa;	                           LEFT JOIN ppa.acao_recurso AS ano3&#xd;&#xa;	                                  ON ano3.ano &#x3d; &#x27;3&#x27;&#xd;&#xa;	                                 AND ano1.cod_acao             &#x3d; ano3.cod_acao&#xd;&#xa;	                                 AND ano1.timestamp_acao_dados &#x3d; ano3.timestamp_acao_dados&#xd;&#xa;	                                 AND ano1.cod_recurso          &#x3d; ano3.cod_recurso&#xd;&#xa;	                           LEFT JOIN ppa.acao_recurso AS ano4&#xd;&#xa;	                                  ON ano4.ano &#x3d; &#x27;4&#x27;&#xd;&#xa;	                                 AND ano1.cod_acao             &#x3d; ano4.cod_acao&#xd;&#xa;	                                 AND ano1.timestamp_acao_dados &#x3d; ano4.timestamp_acao_dados&#xd;&#xa;	                                 AND ano1.cod_recurso          &#x3d; ano4.cod_recurso&#xd;&#xa;	                               WHERE ano1.ano &#x3d; &#x27;1&#x27;&#xd;&#xa;	                            &#x29; AS total_recursos&#xd;&#xa;	                          ON total_recursos.cod_acao &#x3d; acao.cod_acao&#xd;&#xa;	                        JOIN &#x28;&#xd;&#xa;	                                SELECT ano1.cod_acao&#xd;&#xa;	                                     , ano1.timestamp_acao_dados&#xd;&#xa;	                                     , COALESCE&#x28;ano1.valor, 0.00&#x29; AS ano1&#xd;&#xa;	                                     , COALESCE&#x28;ano2.valor, 0.00&#x29; AS ano2&#xd;&#xa;	                                     , COALESCE&#x28;ano3.valor, 0.00&#x29; AS ano3&#xd;&#xa;	                                     , COALESCE&#x28;ano4.valor, 0.00&#x29; AS ano4&#xd;&#xa;	                                FROM ppa.acao_quantidade as ano1&#xd;&#xa;	                           LEFT JOIN ppa.acao_quantidade as ano2&#xd;&#xa;	                                  ON ano2.ano &#x3d; &#x27;2&#x27;&#xd;&#xa;	                                 AND ano1.cod_acao             &#x3d; ano2.cod_acao&#xd;&#xa;	                                 AND ano1.timestamp_acao_dados &#x3d; ano2.timestamp_acao_dados&#xd;&#xa;	                                 AND ano1.cod_recurso          &#x3d; ano2.cod_recurso&#xd;&#xa;	                           LEFT JOIN ppa.acao_quantidade as ano3&#xd;&#xa;	                                  ON ano3.ano &#x3d; &#x27;3&#x27;&#xd;&#xa;	                                 AND ano1.cod_acao             &#x3d; ano3.cod_acao&#xd;&#xa;	                                 AND ano1.timestamp_acao_dados &#x3d; ano3.timestamp_acao_dados&#xd;&#xa;	                                 AND ano1.cod_recurso          &#x3d; ano3.cod_recurso&#xd;&#xa;	                           LEFT JOIN ppa.acao_quantidade as ano4&#xd;&#xa;	                                  ON ano4.ano &#x3d; &#x27;4&#x27;&#xd;&#xa;	                                 AND ano1.cod_acao             &#x3d; ano4.cod_acao&#xd;&#xa;	                                 AND ano1.timestamp_acao_dados &#x3d; ano4.timestamp_acao_dados&#xd;&#xa;	                                 AND ano1.cod_recurso          &#x3d; ano4.cod_recurso&#xd;&#xa;	                               WHERE ano1.ano &#x3d; &#x27;1&#x27;&#xd;&#xa;	                            &#x29; AS total_metas&#xd;&#xa;	                          ON total_metas.cod_acao &#x3d; acao.cod_acao&#xd;&#xa;	                       WHERE NOT EXISTS &#x28;&#xd;&#xa;	                                         SELECT cod_acao&#xd;&#xa;	                                           FROM tcemg.inscricao_acao&#xd;&#xa;	                                          WHERE inscricao_acao.cod_acao &#x3d; acao_dados.cod_acao&#xd;&#xa;	                                            AND inscricao_acao.exercicio &#x3d; acao_dados.exercicio&#xd;&#xa;	                                        &#x29;&#xd;&#xa;	                         AND NOT EXISTS &#x28;&#xd;&#xa;	                                         SELECT 1&#xd;&#xa;	                                           FROM tcemg.arquivo_incamp&#xd;&#xa;	                                          WHERE arquivo_incamp.cod_acao  &#x3d; acao.cod_acao&#xd;&#xa;	                                            AND arquivo_incamp.exercicio &#x3d; stExercicio  &#xd;&#xa;	                                            AND arquivo_incamp.mes &#x3c; stMes&#x3a;&#x3a;int  &#xd;&#xa;	                                        &#x29;&#xd;&#xa;	                         AND NOT EXISTS &#x28;&#xd;&#xa;	                                         SELECT 1&#xd;&#xa;	                                           FROM tcemg.arquivo_amp&#xd;&#xa;	                                          WHERE arquivo_amp.cod_acao  &#x3d; acao.cod_acao&#xd;&#xa;	                                            AND arquivo_amp.exercicio &#x3d; stExercicio   &#xd;&#xa;	                                            AND arquivo_amp.mes &#x3c;&#x3d; stMes&#x3a;&#x3a;int    &#xd;&#xa;	                                        &#x29;&#xd;&#xa;	                    GROUP BY acao.num_acao&#xd;&#xa;	                           , acao.cod_acao&#xd;&#xa;	                           , acao_dados.descricao&#xd;&#xa;	                           , acao_dados.finalidade&#xd;&#xa;	                           , produto.descricao&#xd;&#xa;	                           , unidade_medida.nom_unidade&#xd;&#xa;	                    ORDER BY acao.num_acao&#xd;&#xa;			&#x29;, &#xd;&#xa;			incamp12 as &#xd;&#xa;			&#x28;&#xd;&#xa;						SELECT 12 AS tipo_registro&#xd;&#xa;	                          , LPAD&#x28;stOrgaoIncamp&#x3a;&#x3a;VARCHAR, 2, &#x27;0&#x27;&#x29; AS cod_orgao&#xd;&#xa;	                          , LPAD&#x28;LPAD&#x28;acao_unidade_executora.num_orgao&#x3a;&#x3a;VARCHAR, 2, &#x27;0&#x27;&#x29;&#x7c;&#x7c;LPAD&#x28;acao_unidade_executora.num_unidade&#x3a;&#x3a;VARCHAR, 2, &#x27;0&#x27;&#x29;,5,&#x27;0&#x27;&#x29; AS cod_unidade&#xd;&#xa;	                          , LPAD&#x28;acao_dados.cod_funcao&#x3a;&#x3a;VARCHAR,2,&#x27;0&#x27;&#x29; AS cod_funcao&#xd;&#xa;	                          , LPAD&#x28;acao_dados.cod_subfuncao&#x3a;&#x3a;VARCHAR,3,&#x27;0&#x27;&#x29; AS cod_subfuncao&#xd;&#xa;	                          , LPAD&#x28;programa.num_programa&#x3a;&#x3a;VARCHAR,4,&#x27;0&#x27;&#x29; AS cod_programa&#xd;&#xa;	                          , acao.cod_acao&#xd;&#xa;	                          , LPAD&#x28;acao.num_acao&#x3a;&#x3a;VARCHAR, 4, &#x27;0&#x27;&#x29; AS id_acao&#xd;&#xa;	                          , &#x27; &#x27; AS id_sub_acao&#xd;&#xa;	                          , COALESCE&#x28;&#x28;SELECT ppa.busca_valor_meta_ano_ppa&#x28;acao.cod_acao,&#x27;1&#x27;,acao.ultimo_timestamp_acao_dados&#x29;&#x29;, 0.00&#x29; AS metas_ano_1&#xd;&#xa;	                          , COALESCE&#x28;&#x28;SELECT ppa.busca_valor_meta_ano_ppa&#x28;acao.cod_acao,&#x27;2&#x27;,acao.ultimo_timestamp_acao_dados&#x29;&#x29;, 0.00&#x29; AS metas_ano_2&#xd;&#xa;	                          , COALESCE&#x28;&#x28;SELECT ppa.busca_valor_meta_ano_ppa&#x28;acao.cod_acao,&#x27;3&#x27;,acao.ultimo_timestamp_acao_dados&#x29;&#x29;, 0.00&#x29; AS metas_ano_3&#xd;&#xa;	                          , COALESCE&#x28;&#x28;SELECT ppa.busca_valor_meta_ano_ppa&#x28;acao.cod_acao,&#x27;4&#x27;,acao.ultimo_timestamp_acao_dados&#x29;&#x29;, 0.00&#x29; AS metas_ano_4&#xd;&#xa;	                          , COALESCE&#x28;&#x28;SELECT ppa.busca_valor_recurso_ano_ppa&#x28;acao.cod_acao,&#x27;1&#x27;,acao.ultimo_timestamp_acao_dados&#x29;&#x29;, 0.00&#x29; AS recursos_ano_1&#xd;&#xa;	                          , COALESCE&#x28;&#x28;SELECT ppa.busca_valor_recurso_ano_ppa&#x28;acao.cod_acao,&#x27;2&#x27;,acao.ultimo_timestamp_acao_dados&#x29;&#x29;, 0.00&#x29; AS recursos_ano_2&#xd;&#xa;	                          , COALESCE&#x28;&#x28;SELECT ppa.busca_valor_recurso_ano_ppa&#x28;acao.cod_acao,&#x27;3&#x27;,acao.ultimo_timestamp_acao_dados&#x29;&#x29;, 0.00&#x29; AS recursos_ano_3&#xd;&#xa;	                          , COALESCE&#x28;&#x28;SELECT ppa.busca_valor_recurso_ano_ppa&#x28;acao.cod_acao,&#x27;4&#x27;,acao.ultimo_timestamp_acao_dados&#x29;&#x29;, 0.00&#x29; AS recursos_ano_4&#xd;&#xa;	                       FROM ppa.acao&#xd;&#xa;	                       JOIN ppa.acao_dados&#xd;&#xa;	                         ON acao_dados.cod_acao &#x3d; acao.cod_acao&#xd;&#xa;	                        AND acao_dados.timestamp_acao_dados &#x3d; acao.ultimo_timestamp_acao_dados&#xd;&#xa;	                       JOIN ppa.acao_unidade_executora&#xd;&#xa;	                         ON acao_unidade_executora.cod_acao &#x3d; acao.cod_acao&#xd;&#xa;	                        AND acao_unidade_executora.timestamp_acao_dados &#x3d; acao.ultimo_timestamp_acao_dados&#xd;&#xa;	                       JOIN ppa.programa&#xd;&#xa;	                         ON programa.cod_programa &#x3d; acao.cod_programa&#xd;&#xa;	                      WHERE NOT EXISTS &#x28;&#xd;&#xa;	                                         SELECT cod_acao&#xd;&#xa;	                                           FROM tcemg.inscricao_acao&#xd;&#xa;	                                          WHERE inscricao_acao.cod_acao &#x3d; acao_dados.cod_acao&#xd;&#xa;	                                            AND inscricao_acao.exercicio &#x3d; acao_dados.exercicio&#xd;&#xa;	                                       &#x29;&#xd;&#xa;	                    GROUP BY tipo_registro&#xd;&#xa;	                           , cod_orgao&#xd;&#xa;	                           , cod_unidade&#xd;&#xa;	                           , programa.num_programa&#xd;&#xa;	                           , id_acao&#xd;&#xa;	                           , id_sub_acao&#xd;&#xa;	                           , acao_dados.cod_funcao&#xd;&#xa;	                           , acao.cod_acao&#xd;&#xa;	                           , acao_dados.cod_subfuncao&#xd;&#xa;	                    ORDER BY acao.num_acao			&#xd;&#xa;			&#x29;,&#xd;&#xa;			incamp_format as&#xd;&#xa;			&#x28;&#xd;&#xa;				select&#xd;&#xa;					tipo_registro&#x3a;&#x3a;varchar,&#xd;&#xa;					id_acao, &#xd;&#xa;					tipo_registro&#x3a;&#x3a;varchar&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;possui_sub_acao,&#x27;0&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; Lpad&#x28;coalesce&#x28;id_acao, &#x27;&#x27;&#x29;, 4, &#x27; &#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;substring&#x28;replace&#x28;desc_acao&#x3a;&#x3a;varchar,&#x27;&#x3b;&#x27;,&#x27;&#x7c;&#x27;&#x29;, 1,200&#x29;,&#x27; &#x27;&#x29;					&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;substring&#x28;replace&#x28;finalidade_acao&#x3a;&#x3a;varchar,&#x27;&#x3b;&#x27;,&#x27;&#x7c;&#x27;&#x29;, 1,500&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;substring&#x28;produto&#x3a;&#x3a;varchar, 1,50&#x29;, &#x27;&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;substring&#x28;unidade_medida, 1,15&#x29;, &#x27; &#x27;&#x29;	&#xd;&#xa;					as d&#xd;&#xa;				from incamp10  &#xd;&#xa;				union all&#xd;&#xa;				select&#xd;&#xa;					tipo_registro&#x3a;&#x3a;varchar,&#xd;&#xa;					id_acao,&#xd;&#xa;					tipo_registro&#x3a;&#x3a;varchar&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_orgao,&#x27; &#x27;&#x29;, 2, &#x27; &#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_unidade, &#x27; &#x27;&#x29;, 5, &#x27; &#x27;&#x29;					&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_funcao, &#x27;&#x27;&#x29;, 2, &#x27; &#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_subfuncao,&#x27;&#x27;&#x29;, 3,  &#x27; &#x27;&#x29;	&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; lpad&#x28;coalesce&#x28;cod_programa, &#x27;&#x27;&#x29;, 4, &#x27; &#x27;&#x29;					&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; lpad&#x28;coalesce&#x28;id_acao, &#x27;&#x27;&#x29;, 4, &#x27; &#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; lpad&#x28;coalesce&#x28;id_sub_acao  ,&#x27; &#x27;&#x29;, 4, &#x27; &#x27;&#x29;					&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;replace&#x28;to_char&#x28;metas_ano_1,&#x27;FM99999999990D00&#x27;&#x29;,&#x27;.&#x27;,&#x27;,&#x27;&#x29;,&#x27;0,00&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;replace&#x28;to_char&#x28;metas_ano_2,&#x27;FM99999999990D00&#x27;&#x29;,&#x27;.&#x27;,&#x27;,&#x27;&#x29;,&#x27;0,00&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;replace&#x28;to_char&#x28;metas_ano_3,&#x27;FM99999999990D00&#x27;&#x29;,&#x27;.&#x27;,&#x27;,&#x27;&#x29;,&#x27;0,00&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;replace&#x28;to_char&#x28;metas_ano_4,&#x27;FM99999999990D00&#x27;&#x29;,&#x27;.&#x27;,&#x27;,&#x27;&#x29;,&#x27;0,00&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;replace&#x28;to_char&#x28;recursos_ano_1,&#x27;FM99999999990D00&#x27;&#x29;,&#x27;.&#x27;,&#x27;,&#x27;&#x29;,&#x27;0,00&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;replace&#x28;to_char&#x28;recursos_ano_2,&#x27;FM99999999990D00&#x27;&#x29;,&#x27;.&#x27;,&#x27;,&#x27;&#x29;,&#x27;0,00&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;replace&#x28;to_char&#x28;recursos_ano_3,&#x27;FM99999999990D00&#x27;&#x29;,&#x27;.&#x27;,&#x27;,&#x27;&#x29;,&#x27;0,00&#x27;&#x29;&#xd;&#xa;					&#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;replace&#x28;to_char&#x28;recursos_ano_4,&#x27;FM99999999990D00&#x27;&#x29;,&#x27;.&#x27;,&#x27;,&#x27;&#x29;,&#x27;0,00&#x27;&#x29;&#xd;&#xa;					as d&#xd;&#xa;				from incamp12&#xd;&#xa;			&#x29;&#xd;&#xa;			insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;			select  &#x27;INCAMP.csv&#x27;, json_agg&#x28;d&#x29;&#xd;&#xa;			from &#xd;&#xa;			&#x28;&#xd;&#xa;				select regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;  d  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;,&#x27;&#x5e;&#x3b;&#x27;,&#x27; &#x3b;&#x27;&#x29;,&#x27;&#x3b;&#x3b;&#x27;,&#x27;&#x3b; &#x3b;&#x27;,&#x27;g&#x27;&#x29;,&#x27;&#x3b;&#x24;&#x27;,&#x27;&#x3b; &#x27;&#x29; as d from incamp_format order by id_acao desc ,tipo_registro&#xd;&#xa;			&#x29; x&#x3b;&#xd;&#xa;			&#xd;&#xa;	end if&#x3b;&#xd;&#xa;&#xd;&#xa;    if strpos&#x28;stArquivos,&#x27;&#x22;INCPRO.csv&#x22;&#x27;&#x29; &#x3e; 0&#xd;&#xa;		then&#xd;&#xa;		&#xd;&#xa;			with incpro as&#xd;&#xa;			&#x28;&#xd;&#xa;				SELECT &#x2a;&#xd;&#xa;	                &#xd;&#xa;	                FROM tcemg.recupera_ppa_inclusao_programa&#x28;stExercicio, stDtfim&#x29;  	                &#xd;&#xa;	                  AS retorno &#x28; cod_programa        INTEGER&#xd;&#xa;	                             , ppa_cod_programa    INTEGER&#xd;&#xa;	                             , nom_programa        VARCHAR&#xd;&#xa;	                             , objetivo            VARCHAR&#xd;&#xa;	                             , total_recurso_1_ano VARCHAR&#xd;&#xa;	                             , total_recurso_2_ano VARCHAR&#xd;&#xa;	                             , total_recurso_3_ano VARCHAR&#xd;&#xa;	                             , total_recurso_4_ano VARCHAR&#xd;&#xa;	                             , numero_lei          INTEGER&#xd;&#xa;	                             , data_lei            VARCHAR&#xd;&#xa;	                             , data_publicacao_lei VARCHAR&#xd;&#xa;	                             &#x29;&#xd;&#xa;			&#x29;       &#xd;&#xa;			, incpro_format as &#xd;&#xa;			&#x28;&#xd;&#xa;			    select &#xd;&#xa;			    		   coalesce&#x28;cod_programa&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;			       &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;substring&#x28;replace&#x28;nom_programa&#x3a;&#x3a;varchar,&#x27;&#x3b;&#x27;,&#x27;&#x7c;&#x27;&#x29;, 1,200&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;			       &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;substring&#x28;replace&#x28;objetivo&#x3a;&#x3a;varchar     ,&#x27;&#x3b;&#x27;,&#x27;&#x7c;&#x27;&#x29;, 1,500&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;total_recurso_1_ano&#x3a;&#x3a;varchar, &#x27;0,00&#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;total_recurso_2_ano&#x3a;&#x3a;varchar, &#x27;0,00&#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;total_recurso_3_ano&#x3a;&#x3a;varchar, &#x27;0,00&#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;total_recurso_4_ano&#x3a;&#x3a;varchar, &#x27;0,00&#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;numero_lei&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;data_lei&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;data_publicacao_lei&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;				   as registro_incpro&#xd;&#xa;			    from incpro&#xd;&#xa;			&#x29;&#xd;&#xa;			insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;			select  &#x27;INCPRO.csv&#x27;, json_agg&#x28;     regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;  registro_incpro  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;,&#x27;&#x5e;&#x3b;&#x27;,&#x27; &#x3b;&#x27;&#x29;,&#x27;&#x3b;&#x3b;&#x27;,&#x27;&#x3b; &#x3b;&#x27;,&#x27;g&#x27;&#x29;,&#x27;&#x3b;&#x24;&#x27;,&#x27;&#x3b; &#x27;&#x29;     &#x29; from incpro_format&#x3b;			&#xd;&#xa;			&#xd;&#xa;	end if&#x3b;&#xd;&#xa;&#xd;&#xa;    if strpos&#x28;stArquivos,&#x27;&#x22;IUOC.csv&#x22;&#x27;&#x29; &#x3e; 0&#xd;&#xa;		then 	&#xd;&#xa;			&#xd;&#xa;					-- &#x2a;&#x2a;&#x2a;&#x2a;&#x2a;     Criar tabela intermedi&#xe1;ria     &#x2a;&#x2a;&#x2a;&#x2a;&#x2a;&#xd;&#xa;					CREATE TABLE prestacao_contas.tabiuoc &#x28;&#xd;&#xa;								codorgao text NULL,&#xd;&#xa;								codunidadesub text NULL,&#xd;&#xa;								idfundo text NULL,&#xd;&#xa;								descunidadesub varchar&#x28;60&#x29; NULL,&#xd;&#xa;								esubunidade int4 NULL,&#xd;&#xa;								num_orgao int4 NULL,&#xd;&#xa;								num_unidade int4 NULL&#xd;&#xa;								&#x29;&#xd;&#xa;								WITH &#x28;&#xd;&#xa;									OIDS&#x3d;FALSE&#xd;&#xa;								&#x29; &#x3b;&#xd;&#xa;		&#xd;&#xa;		&#xd;&#xa;			-- &#x2a;&#x2a;&#x2a;&#x2a;&#x2a;     Extra&#xe7;&#xe3;o das informa&#xe7;&#xf5;es para a tabela intermedi&#xe1;ria     &#x2a;&#x2a;&#x2a;&#x2a;&#x2a;&#xd;&#xa;			FOR reRecPar IN	select &#x28;UNNEST&#x28;ARRAY&#x5b;string_to_array&#x28;stOrgao, &#x27;,&#x27;&#x29;&#x5d;&#x29;&#x3a;&#x3a;VARCHAR&#x29; as ParValorLoop  &#xd;&#xa;		    LOOP&#xd;&#xa;		    &#xd;&#xa;		        insert into prestacao_contas.tabiuoc&#xd;&#xa;				SELECT  lpad&#x28;reRecPar.ParValorLoop&#x3a;&#x3a;VARCHAR,2,&#x27;0&#x27;&#x29; AS codorgao&#xd;&#xa;                    , lpad&#x28;lpad&#x28;uniorcam.num_orgao&#x3a;&#x3a;VARCHAR, 2, &#x27;0&#x27;&#x29;&#x7c;&#x7c;lpad&#x28;uniorcam.num_unidade&#x3a;&#x3a;VARCHAR, 2, &#x27;0&#x27;&#x29;,5,&#x27;0&#x27;&#x29; AS codunidadesub&#xd;&#xa;                    , CASE WHEN uniorcam.identificador &#x3d; 1 OR uniorcam.identificador &#x3d; 2 OR uniorcam.identificador &#x3d; 3 OR&#xd;&#xa;                                uniorcam.identificador &#x3d; 4 OR uniorcam.identificador &#x3d; 99&#xd;&#xa;                            THEN lpad&#x28;uniorcam.identificador&#x3a;&#x3a;VARCHAR, 2,&#x27;0&#x27;&#x29;&#xd;&#xa;                            ELSE &#x27;&#x27;&#xd;&#xa;                        END AS idfundo&#xd;&#xa;                    , unidade.nom_unidade AS descunidadesub&#xd;&#xa;                    , 2 AS esubunidade&#xd;&#xa;                    , uniorcam.num_orgao&#xd;&#xa;                    , uniorcam.num_unidade&#xd;&#xa;                 FROM tcemg.uniorcam&#xd;&#xa;                 JOIN orcamento.orgao&#xd;&#xa;                   ON orgao.num_orgao &#x3d; uniorcam.num_orgao&#xd;&#xa;                  AND orgao.exercicio &#x3d; uniorcam.exercicio&#xd;&#xa;                 JOIN orcamento.unidade&#xd;&#xa;                   ON unidade.num_unidade &#x3d; uniorcam.num_unidade&#xd;&#xa;                  AND unidade.num_orgao &#x3d; uniorcam.num_orgao&#xd;&#xa;                  AND unidade.exercicio &#x3d; uniorcam.exercicio&#xd;&#xa;                WHERE uniorcam.exercicio &#x3d; stExercicio      --&#x27;2015&#x27;&#xd;&#xa;                  AND NOT EXISTS &#x28;&#xd;&#xa;                                     SELECT 1&#xd;&#xa;                                       FROM tcemg.arquivo_uoc&#xd;&#xa;                                      WHERE arquivo_uoc.num_orgao   &#x3d; uniorcam.num_orgao&#xd;&#xa;                                        AND arquivo_uoc.num_unidade &#x3d; uniorcam.num_unidade&#xd;&#xa;                                        AND arquivo_uoc.exercicio   &#x3d; stExercicio    --&#x27;2015&#x27; &#xd;&#xa;                                    &#x29;&#xd;&#xa;                  AND NOT EXISTS &#x28;&#xd;&#xa;                                     SELECT 1&#xd;&#xa;                                       FROM tcemg.arquivo_iuoc&#xd;&#xa;                                      WHERE arquivo_iuoc.num_orgao   &#x3d; uniorcam.num_orgao&#xd;&#xa;                                        AND arquivo_iuoc.num_unidade &#x3d; uniorcam.num_unidade&#xd;&#xa;                                        AND arquivo_iuoc.exercicio   &#x3d; stExercicio    --&#x27;2015&#x27;   &#xd;&#xa;                                        AND arquivo_iuoc.mes         &#x3c;  stMes&#x3a;&#x3a;int --01   &#xd;&#xa;                                    &#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;			end LOOP&#x3b;&#xd;&#xa;		&#xd;&#xa;			with iuoc as&#xd;&#xa;			&#x28;&#xd;&#xa;				SELECT &#xd;&#xa;						  codorgao&#xd;&#xa;						, codunidadesub&#xd;&#xa;						, idfundo&#xd;&#xa;						, descunidadesub&#xd;&#xa;						, esubunidade&#xd;&#xa;						, num_orgao&#xd;&#xa;						, num_unidade&#xd;&#xa;				FROM prestacao_contas.tabiuoc&#xd;&#xa;			&#x29;       &#xd;&#xa;			, iuoc_format as &#xd;&#xa;			&#x28;&#xd;&#xa;			    select &#xd;&#xa;			               coalesce&#x28;codorgao&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;			       &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;lpad&#x28;case when codunidadesub&#x3a;&#x3a;varchar &#x21;&#x3d; &#x27;&#x27; then codunidadesub&#x3a;&#x3a;varchar else null end,5,&#x27;0&#x27;&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;lpad&#x28;case when idfundo&#x3a;&#x3a;varchar &#x21;&#x3d; &#x27;&#x27; then idfundo&#x3a;&#x3a;varchar else null end,2,&#x27;0&#x27;&#x29;,&#x27; &#x27;&#x29;			       &#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;substring&#x28;replace&#x28;descunidadesub&#x3a;&#x3a;varchar,&#x27;&#x3b;&#x27;,&#x27;&#x7c;&#x27;&#x29;,1,50&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;esubunidade&#x3a;&#x3a;varchar,&#x27;0&#x27;&#x29;&#xd;&#xa;			       as registro_iuoc&#xd;&#xa;			    from iuoc    			&#xd;&#xa;			&#x29;&#xd;&#xa;  			insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;			select  &#x27;IUOC.csv&#x27;, json_agg&#x28;     regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;  registro_iuoc  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;,&#x27;&#x5e;&#x3b;&#x27;,&#x27; &#x3b;&#x27;&#x29;,&#x27;&#x3b;&#x3b;&#x27;,&#x27;&#x3b; &#x3b;&#x27;,&#x27;g&#x27;&#x29;,&#x27;&#x3b;&#x24;&#x27;,&#x27;&#x3b; &#x27;&#x29;     &#x29; from iuoc_format&#x3b;   &#xd;&#xa;&#xd;&#xa;		    --&#x2a;&#x2a;&#x2a;&#x2a;  Exclui tabela intermedi&#xe1;ria   &#x2a;&#x2a;&#x2a;&#x2a;&#xd;&#xa;		    DROP  TABLE prestacao_contas.tabiuoc&#x3b;&#xd;&#xa;			&#xd;&#xa;	end if&#x3b;&#xd;&#xa;&#xd;&#xa;    update prestacao_contas.fila_relatorio &#xd;&#xa;    	set &#xd;&#xa;    		  resposta &#x3d; &#x28;select  json_object_agg&#x28;t.arquivo,t.content&#x29; from tmp_table t&#x29;&#xd;&#xa;      	where id &#x3d; inId&#x3b;&#xd;&#xa;   		&#xd;&#xa;    DROP TABLE tmp_table&#x3b;&#xd;&#xa;    &#xd;&#xa;    return stResultado&#x3b;&#xd;&#xa;&#xd;&#xa; EXCEPTION  &#xd;&#xa;   	when others then&#xd;&#xa;   	    &#xd;&#xa;	    stError &#x3a;&#x3d; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;SQLErro&#x27;, REPLACE&#x28; SQLERRM, &#x27;&#x22;&#x27;, &#x27;&#x2a;&#x27;&#x29;&#x29;&#x3b;&#xd;&#xa;	    stError &#x3a;&#x3d; &#x27;&#x27; &#x7c;&#x7c; stError &#x7c;&#x7c; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;Exercicio&#x27;, COALESCE&#x28;stExercicio&#x3a;&#x3a;varchar,&#x27;&#x27;&#x29;&#x29;&#x3b;&#xd;&#xa;	    stError &#x3a;&#x3d; &#x27;&#x27; &#x7c;&#x7c; stError &#x7c;&#x7c; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;Mes&#x27;, COALESCE&#x28;stMes&#x3a;&#x3a;text,&#x27;&#x27;&#x29;&#x29;&#x3b;	    &#xd;&#xa;	    stError &#x3a;&#x3d; &#x27;&#x27; &#x7c;&#x7c; stError &#x7c;&#x7c; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;Entidade&#x27;, COALESCE&#x28;stEntidade&#x3a;&#x3a;varchar,&#x27;&#x27;&#x29;&#x29;&#x3b;	    &#xd;&#xa;	    stError &#x3a;&#x3d; &#x27;&#x27; &#x7c;&#x7c; stError &#x7c;&#x7c; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;Orgao&#x27;, COALESCE&#x28;stOrgao&#x3a;&#x3a;varchar,&#x27;&#x27;&#x29;&#x29;&#x3b;	    &#xd;&#xa;	    stError &#x3a;&#x3d; &#x27;&#x27; &#x7c;&#x7c; stError &#x7c;&#x7c; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;Dtinicio&#x27;, &#x27;N_UTIL&#x27;&#x29;&#x3b;   &#xd;&#xa;	    stError &#x3a;&#x3d; &#x27;&#x27; &#x7c;&#x7c; stError &#x7c;&#x7c; prestacao_contas.getVariablesKeyAndValue&#x28;&#x27;Dtfim&#x27;, COALESCE&#x28;stDtfim&#x3a;&#x3a;varchar, &#x27;&#x27;&#x29;&#x29;&#x3b;&#xd;&#xa;   		stResultado &#x3a;&#x3d; &#x27;nok&#x27;&#x3b;&#xd;&#xa;   		&#xd;&#xa;   		stError &#x3a;&#x3d; prestacao_contas.processa_status_erro&#x28;inId, stError&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;   		Return stResultado&#x3b;	    	&#xd;&#xa;&#xd;&#xa;   &#xd;&#xa;END&#x3b;&#xd;&#xa;&#x24;function&#x24;&#xd;&#xa;</sql>
