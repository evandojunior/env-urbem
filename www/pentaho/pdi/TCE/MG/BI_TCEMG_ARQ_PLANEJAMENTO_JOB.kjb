    <entry>
      <name>SJ_010_ESS_CREATE_SCHEMA_FUNCTION</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;CREATE SCHEMA IF NOT EXISTS  bi&#x3b;&#xd;&#xa;SET search_path &#x3d; &#x27;bi&#x27;, pg_catalog&#x3b;&#xd;&#xa;&#xd;&#xa;CREATE OR REPLACE FUNCTION bi.tcemg_sicom_instrumentos_planejamento&#x28;bigint&#x29;&#xd;&#xa; RETURNS text&#xd;&#xa; LANGUAGE plpgsql&#xd;&#xa;AS &#x24;function&#x24;&#xd;&#xa;DECLARE&#xd;&#xa;    inId ALIAS FOR &#x24;1&#x3b;&#xd;&#xa;    stParametros 	VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stEntidade		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stExercicio		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stMes			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stArquivos 		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stSql     		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stSql2 			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stDtinicio		VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa;	stDtfim			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa; 	reRegistro       RECORD&#x3b;	&#xd;&#xa; 	stResultado		VARCHAR &#x3a;&#x3d; &#x27;ok&#x27;&#x3b;&#xd;&#xa; 	stError			VARCHAR &#x3a;&#x3d; &#x27;&#x27;&#x3b;&#xd;&#xa; 	&#xd;&#xa;begin&#xd;&#xa;	&#xd;&#xa;	select &#xd;&#xa;		valor &#xd;&#xa;	into stParametros &#xd;&#xa;	from prestacao_contas.fila_relatorio &#xd;&#xa;	where id &#x3d; inId&#x3b;&#xd;&#xa;	&#xd;&#xa;	stSql &#x3a;&#x3d; &#x27;select &#x2a; from json_to_record&#x28;&#x27;&#x27;&#x27;&#x7c;&#x7c;stParametros&#x7c;&#x7c;&#x27;	&#x27;&#x27;&#x29;  as x&#x28;entidade text, mes int, exercicio int, arquivo text, data_inicio text, data_fim text&#x29;&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;	FOR reRegistro IN EXECUTE stSql&#xd;&#xa;    LOOP		&#xd;&#xa;		stEntidade	&#x3a;&#x3d; reRegistro.entidade&#x3b;&#xd;&#xa;		stExercicio	&#x3a;&#x3d; reRegistro.exercicio&#x3b;&#xd;&#xa;		stMes		&#x3a;&#x3d; reRegistro.mes&#x3b;&#xd;&#xa;		stArquivos 	&#x3a;&#x3d; reRegistro.arquivo&#x3b;&#xd;&#xa;		stDtinicio	&#x3a;&#x3d; reRegistro.data_inicio&#x3b;		&#xd;&#xa;		stDtfim		&#x3a;&#x3d; reRegistro.data_fim&#x3b;	&#xd;&#xa;    END LOOP&#x3b;&#xd;&#xa;    &#xd;&#xa;    CREATE TEMPORARY TABLE tmp_table &#x28;arquivo varchar&#x28;200&#x29;, content json&#x29;&#x3b;&#xd;&#xa;    &#xd;&#xa;    if strpos&#x28;stArquivos,&#x27;&#x22;LPP.csv&#x22;&#x27;&#x29; &#x3e; 0&#xd;&#xa;	 	then  &#xd;&#xa;	 		&#xd;&#xa;			&#xd;&#xa;			with lpp as&#xd;&#xa;			&#x28;&#xd;&#xa;			    SELECT  ppa_alteracao.nroleialteracao AS nroleialteracao&#xd;&#xa;			              , TO_CHAR&#x28;ppa_alteracao.dataleialteracao, &#x27;DDMMYYYY&#x27;&#x29; AS dataleialteracao&#xd;&#xa;			              , TO_CHAR&#x28;ppa_alteracao.datapubleialt, &#x27;DDMMYYYY&#x27;&#x29; AS datapubleialt&#xd;&#xa;			              , ppa_anterior.nroLeiPPA&#xd;&#xa;			              , TO_CHAR&#x28;ppa_anterior.dataLeiPPA, &#x27;DDMMYYYY&#x27;&#x29; AS dataLeiPPA&#xd;&#xa;			              , TO_CHAR&#x28;ppa_anterior.dataPubLeiPPA, &#x27;DDMMYYYY&#x27;&#x29; AS dataPubLeiPPA&#xd;&#xa;			        FROM tcemg.configuracao_leis_ppa&#xd;&#xa;			        JOIN normas.norma&#xd;&#xa;			          ON norma.cod_norma &#x3d; configuracao_leis_ppa.cod_norma&#xd;&#xa;			        JOIN&#x28;  SELECT num_norma AS nroLeiPPA&#xd;&#xa;			                     , dt_assinatura AS dataLeiPPA&#xd;&#xa;			                     , dt_publicacao AS dataPubLeiPPA&#xd;&#xa;			                     , configuracao_leis_ppa.exercicio&#xd;&#xa;			      FROM tcemg.configuracao_leis_ppa&#xd;&#xa;			      JOIN normas.norma&#xd;&#xa;			                    ON norma.cod_norma &#x3d; configuracao_leis_ppa.cod_norma&#xd;&#xa;			                 WHERE  configuracao_leis_ppa.status &#x3c;&#x3e; &#x27;f&#x27;&#xd;&#xa;			                   AND configuracao_leis_ppa.tipo_configuracao &#x3d; &#x27;consulta&#x27;&#xd;&#xa;			                &#x29; as ppa_anterior&#xd;&#xa;			               ON ppa_anterior.exercicio &#x3d; configuracao_leis_ppa.exercicio&#xd;&#xa;			              AND ppa_anterior.dataLeiPPA &#x3d; norma.dt_assinatura&#xd;&#xa;			              AND ppa_anterior.dataPubLeiPPA &#x3d; norma.dt_publicacao&#xd;&#xa;			              AND ppa_anterior.nroLeiPPA &#x3d; norma.num_norma&#xd;&#xa;			       LEFT JOIN&#x28;   SELECT norma.num_norma AS nroleialteracao&#xd;&#xa;			                         , norma.dt_assinatura AS dataleialteracao&#xd;&#xa;			                         , norma.dt_publicacao AS datapubleialt&#xd;&#xa;			                         , configuracao_leis_ppa.exercicio&#xd;&#xa;			                      FROM tcemg.configuracao_leis_ppa&#xd;&#xa;			          JOIN normas.norma&#xd;&#xa;			                        ON norma.cod_norma &#x3d; configuracao_leis_ppa.cod_norma&#xd;&#xa;			                     WHERE configuracao_leis_ppa.tipo_configuracao &#x3d; &#x27;alteracao&#x27;&#xd;&#xa;			                   AND configuracao_leis_ppa.status &#x3c;&#x3e; &#x27;f&#x27;&#xd;&#xa;			                   AND &#x28;SELECT MAX&#x28;norma.dt_publicacao&#x29;&#xd;&#xa;			                        FROM tcemg.configuracao_leis_ppa&#xd;&#xa;			                        JOIN normas.norma&#xd;&#xa;			                          ON norma.cod_norma &#x3d; configuracao_leis_ppa.cod_norma&#xd;&#xa;			                        WHERE configuracao_leis_ppa.tipo_configuracao &#x3d; &#x27;alteracao&#x27;&#xd;&#xa;			                          AND configuracao_leis_ppa.status &#x3c;&#x3e; &#x27;f&#x27;&#x29; &#x3d; norma.dt_publicacao&#xd;&#xa;			            &#x29; as ppa_alteracao&#xd;&#xa;			           ON ppa_anterior.exercicio &#x3d; configuracao_leis_ppa.exercicio&#xd;&#xa;			        WHERE configuracao_leis_ppa.exercicio &#x3d; stExercicio&#xd;&#xa;			          AND configuracao_leis_ppa.status &#x3c;&#x3e; &#x27;f&#x27;&#xd;&#xa;			&#x29;       &#xd;&#xa;			, lpp_format as &#xd;&#xa;			&#x28;&#xd;&#xa;			    select &#xd;&#xa;			       coalesce&#x28;nroLeiPPA&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;			       &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;dataLeiPPA&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;dataPubLeiPPA&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;nroleialteracao&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;dataleialteracao&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;datapubleialt&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29; &#xd;&#xa;			       as registro_lpp&#xd;&#xa;			    from lpp&#xd;&#xa;			&#x29;&#xd;&#xa;			insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;			select  &#x27;LPP.csv&#x27;, json_agg&#x28;     regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;  registro_lpp  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;,&#x27;&#x5e;&#x3b;&#x27;,&#x27; &#x3b;&#x27;&#x29;,&#x27;&#x3b;&#x3b;&#x27;,&#x27;&#x3b; &#x3b;&#x27;,&#x27;g&#x27;&#x29;,&#x27;&#x3b;&#x24;&#x27;,&#x27;&#x3b; &#x27;&#x29;     &#x29; from lpp_format&#x3b;   &#xd;&#xa;	end if&#x3b;&#xd;&#xa;&#xd;&#xa;    if strpos&#x28;stArquivos,&#x27;&#x22;LDO.csv&#x22;&#x27;&#x29; &#x3e; 0&#xd;&#xa;		then &#xd;&#xa;			&#xd;&#xa;			&#xd;&#xa;			with ldo as&#xd;&#xa;			&#x28;&#xd;&#xa;			        SELECT  ldo_alteracao.numero_ldo AS numero_lei_alteracao&#xd;&#xa;			                  , TO_CHAR&#x28;ldo_alteracao.data_ldo, &#x27;DDMMYYYY&#x27;&#x29; AS data_lei_alteracao&#xd;&#xa;			                  , TO_CHAR&#x28;ldo_alteracao.data_publicacao_ldo, &#x27;DDMMYYYY&#x27;&#x29; AS data_publicacao_lei_alteracao&#xd;&#xa;			                  , ldo_anterior.numero_ldo&#xd;&#xa;			                  , TO_CHAR&#x28;ldo_anterior.data_ldo, &#x27;DDMMYYYY&#x27;&#x29; AS data_ldo&#xd;&#xa;			                  , TO_CHAR&#x28;ldo_anterior.data_publicacao_ldo, &#x27;DDMMYYYY&#x27;&#x29; AS data_publicacao_ldo&#xd;&#xa;			            FROM tcemg.configuracao_leis_ldo&#xd;&#xa;			            JOIN normas.norma&#xd;&#xa;			              ON norma.cod_norma &#x3d; configuracao_leis_ldo.cod_norma&#xd;&#xa;			       LEFT JOIN &#x28;SELECT num_norma AS numero_ldo&#xd;&#xa;			                       , dt_assinatura AS data_ldo&#xd;&#xa;			                       , dt_publicacao AS data_publicacao_ldo&#xd;&#xa;			                       , configuracao_leis_ldo.exercicio&#xd;&#xa;			                    FROM tcemg.configuracao_leis_ldo&#xd;&#xa;			                    JOIN normas.norma&#xd;&#xa;			                      ON norma.cod_norma &#x3d; configuracao_leis_ldo.cod_norma&#xd;&#xa;			                 WHERE configuracao_leis_ldo.tipo_configuracao &#x3d; &#x27;consulta&#x27;&#xd;&#xa;			                   AND configuracao_leis_ldo.status &#x3c;&#x3e; &#x27;f&#x27;&#xd;&#xa;			                &#x29; as ldo_anterior&#xd;&#xa;			              ON ldo_anterior.exercicio &#x3d; configuracao_leis_ldo.exercicio&#xd;&#xa;			             AND ldo_anterior.data_ldo &#x3d; norma.dt_assinatura&#xd;&#xa;			             AND ldo_anterior.data_publicacao_ldo &#x3d; norma.dt_publicacao&#xd;&#xa;			             AND ldo_anterior.numero_ldo &#x3d; norma.num_norma&#xd;&#xa;			       LEFT JOIN &#x28;SELECT num_norma AS numero_ldo&#xd;&#xa;			                       , dt_assinatura AS data_ldo&#xd;&#xa;			                       , dt_publicacao AS data_publicacao_ldo&#xd;&#xa;			                       , configuracao_leis_ldo.exercicio&#xd;&#xa;			                    FROM tcemg.configuracao_leis_ldo&#xd;&#xa;			                    JOIN normas.norma&#xd;&#xa;			                      ON norma.cod_norma &#x3d; configuracao_leis_ldo.cod_norma&#xd;&#xa;			                 WHERE configuracao_leis_ldo.tipo_configuracao &#x3d; &#x27;alteracao&#x27;&#xd;&#xa;			                   AND configuracao_leis_ldo.status &#x3c;&#x3e; &#x27;f&#x27;&#xd;&#xa;			                &#x29; as ldo_alteracao&#xd;&#xa;			              ON ldo_anterior.exercicio &#x3d; configuracao_leis_ldo.exercicio&#xd;&#xa;			             AND ldo_alteracao.data_ldo &#x3d; norma.dt_assinatura&#xd;&#xa;			             AND ldo_alteracao.data_publicacao_ldo &#x3d; norma.dt_publicacao&#xd;&#xa;			             AND ldo_alteracao.numero_ldo &#x3d; norma.num_norma&#xd;&#xa;			            WHERE configuracao_leis_ldo.exercicio &#x3d; stExercicio &#xd;&#xa;			              AND configuracao_leis_ldo.status &#x3c;&#x3e; &#x27;f&#x27;&#xd;&#xa;			&#x29;       &#xd;&#xa;			, ldo_format as &#xd;&#xa;			&#x28;&#xd;&#xa;				select &#xd;&#xa;			       coalesce&#x28;numero_ldo,&#x27; &#x27;&#x29;&#xd;&#xa;			       &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;data_ldo&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;data_publicacao_ldo&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;numero_lei_alteracao&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;data_lei_alteracao&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;data_publicacao_lei_alteracao&#x3a;&#x3a;varchar,&#x27; &#x27;&#x29;&#xd;&#xa;			       as registro_ldo&#xd;&#xa;			    from ldo&#xd;&#xa;			&#xd;&#xa;			&#x29;&#xd;&#xa;			insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;			select  &#x27;LDO.csv&#x27;, json_agg&#x28;     regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;  registro_ldo  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;,&#x27;&#x5e;&#x3b;&#x27;,&#x27; &#x3b;&#x27;&#x29;,&#x27;&#x3b;&#x3b;&#x27;,&#x27;&#x3b; &#x3b;&#x27;,&#x27;g&#x27;&#x29;,&#x27;&#x3b;&#x24;&#x27;,&#x27;&#x3b; &#x27;&#x29;     &#x29; from ldo_format&#x3b;			&#xd;&#xa;	end if&#x3b;&#xd;&#xa;&#xd;&#xa;    if strpos&#x28;stArquivos,&#x27;&#x22;UOC.csv&#x22;&#x27;&#x29; &#x3e; 0&#xd;&#xa;		then&#xd;&#xa;			&#xd;&#xa;			&#xd;&#xa;			with uoc as&#xd;&#xa;			&#x28;&#xd;&#xa;			    SELECT  lpad&#x28;&#x28;SELECT valor&#xd;&#xa;			        FROM administracao.configuracao_entidade&#xd;&#xa;			       WHERE configuracao_entidade.cod_entidade IN &#x28;2&#x29;  &#xd;&#xa;			      AND configuracao_entidade.exercicio &#x3d; stExercicio&#xd;&#xa;			    AND configuracao_entidade.parametro &#x3d; &#x27;tcemg_codigo_orgao_entidade_sicom&#x27;&#x29;&#x3a;&#x3a;VARCHAR,2,&#x27;0&#x27;&#x29; AS codorgao&#xd;&#xa;			            , lpad&#x28;lpad&#x28;uniorcam.num_orgao&#x3a;&#x3a;VARCHAR, 2, &#x27;0&#x27;&#x29;&#x7c;&#x7c;lpad&#x28;uniorcam.num_unidade&#x3a;&#x3a;VARCHAR, 2, &#x27;0&#x27;&#x29;,5,&#x27;0&#x27;&#x29; AS codunidadesub&#xd;&#xa;			            , CASE WHEN uniorcam.identificador &#x3d; 1 OR uniorcam.identificador &#x3d; 2 OR uniorcam.identificador &#x3d; 3 OR&#xd;&#xa;			                        uniorcam.identificador &#x3d; 4 OR uniorcam.identificador &#x3d; 99&#xd;&#xa;			                    THEN lpad&#x28;uniorcam.identificador&#x3a;&#x3a;VARCHAR, 2,&#x27;0&#x27;&#x29;&#xd;&#xa;			                    ELSE &#x27;&#x27;&#xd;&#xa;			                END AS idfundo&#xd;&#xa;			            , unidade.nom_unidade AS descunidadesub&#xd;&#xa;			            , 2 AS esubunidade&#xd;&#xa;			            , uniorcam.num_orgao&#xd;&#xa;			            , uniorcam.num_unidade&#xd;&#xa;			         FROM tcemg.uniorcam&#xd;&#xa;			         JOIN orcamento.orgao&#xd;&#xa;			           ON orgao.num_orgao &#x3d; uniorcam.num_orgao&#xd;&#xa;			          AND orgao.exercicio &#x3d; uniorcam.exercicio&#xd;&#xa;			         JOIN orcamento.unidade&#xd;&#xa;			           ON unidade.num_unidade &#x3d; uniorcam.num_unidade&#xd;&#xa;			          AND unidade.num_orgao &#x3d; uniorcam.num_orgao&#xd;&#xa;			          AND unidade.exercicio &#x3d; uniorcam.exercicio&#xd;&#xa;			        WHERE uniorcam.exercicio &#x3d; stExercicio&#xd;&#xa;			          AND NOT EXISTS &#x28;&#xd;&#xa;			                             SELECT 1&#xd;&#xa;			                               FROM tcemg.arquivo_uoc&#xd;&#xa;			                              WHERE arquivo_uoc.num_orgao   &#x3d; uniorcam.num_orgao&#xd;&#xa;			                                AND arquivo_uoc.num_unidade &#x3d; uniorcam.num_unidade&#xd;&#xa;			                                AND arquivo_uoc.exercicio   &#x3d; stExercicio&#xd;&#xa;			                                AND arquivo_uoc.mes &#x3c; stMes&#x3a;&#x3a;int&#xd;&#xa;			                            &#x29;&#xd;&#xa;			&#x29;       &#xd;&#xa;			, uoc_format as &#xd;&#xa;			&#x28;&#xd;&#xa;			    select &#xd;&#xa;			               coalesce&#x28;lpad&#x28;case when codorgao&#x3a;&#x3a;varchar &#x21;&#x3d; &#x27;&#x27; then codorgao&#x3a;&#x3a;varchar else null end,2,&#x27;0&#x27;&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;			       &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;lpad&#x28;case when codunidadesub&#x3a;&#x3a;varchar &#x21;&#x3d; &#x27;&#x27; then codunidadesub&#x3a;&#x3a;varchar else null end,5,&#x27;0&#x27;&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;lpad&#x28;case when idfundo&#x3a;&#x3a;varchar &#x21;&#x3d; &#x27;&#x27; then idfundo&#x3a;&#x3a;varchar else null end,2,&#x27;0&#x27;&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;substring&#x28;replace&#x28;descunidadesub&#x3a;&#x3a;varchar,&#x27;&#x3b;&#x27;,&#x27;&#x7c;&#x27;&#x29;,1,50&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;to_char&#x28;esubunidade,&#x27;FM9999999&#x27;&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;			       as registro_uoc&#xd;&#xa;			    from uoc&#xd;&#xa;			&#x29;&#xd;&#xa;			insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;			select  &#x27;UOC.csv&#x27;, json_agg&#x28;     regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;  registro_uoc  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;,&#x27;&#x5e;&#x3b;&#x27;,&#x27; &#x3b;&#x27;&#x29;,&#x27;&#x3b;&#x3b;&#x27;,&#x27;&#x3b; &#x3b;&#x27;,&#x27;g&#x27;&#x29;,&#x27;&#x3b;&#x24;&#x27;,&#x27;&#x3b; &#x27;&#x29;     &#x29; from uoc_format&#x3b;&#xd;&#xa;	end if&#x3b;&#xd;&#xa;&#xd;&#xa;    if strpos&#x28;stArquivos,&#x27;&#x22;PRO.csv&#x22;&#x27;&#x29; &#x3e; 0&#xd;&#xa;		then 	&#xd;&#xa;			&#xd;&#xa;			&#xd;&#xa;			with pro as&#xd;&#xa;			&#x28;&#xd;&#xa;			    SELECT  &#x2a;&#xd;&#xa;			        FROM tcemg.recupera_ppa_programa&#x28;stExercicio, false&#x29; &#xd;&#xa;			            AS retorno &#x28;&#xd;&#xa;			                         cod_programa         INTEGER&#xd;&#xa;			                        ,num_programa         INTEGER&#xd;&#xa;			                        ,nome_programa        VARCHAR&#xd;&#xa;			                        ,objetivo             VARCHAR&#xd;&#xa;			                        ,total_recursos_ano_1 VARCHAR&#xd;&#xa;			                        ,total_recursos_ano_2 VARCHAR&#xd;&#xa;			                        ,total_recursos_ano_3 VARCHAR&#xd;&#xa;			                        ,total_recursos_ano_4 VARCHAR&#xd;&#xa;			                        &#x29;&#xd;&#xa;			        ORDER BY num_programa&#xd;&#xa;			&#x29;       &#xd;&#xa;			, pro_format as &#xd;&#xa;			&#x28;&#xd;&#xa;			    select &#xd;&#xa;			    		   coalesce&#x28;to_char&#x28;num_programa, &#x27;FM0000&#x27;&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;			       &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;substring&#x28;replace&#x28;nome_programa&#x3a;&#x3a;varchar,&#x27;&#x3b;&#x27;,&#x27;&#x7c;&#x27;&#x29;, 1,200&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;substring&#x28;replace&#x28;objetivo&#x3a;&#x3a;varchar     ,&#x27;&#x3b;&#x27;,&#x27;&#x7c;&#x27;&#x29;, 1,500&#x29;,&#x27; &#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;total_recursos_ano_1&#x3a;&#x3a;varchar, &#x27;0,00&#x27;&#x29;&#xd;&#xa;				   &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#