a&#xd;&#xa;			              JOIN orcamento.receita&#xd;&#xa;			                ON receita.cod_receita &#x3d; previsao_receita.cod_receita&#xd;&#xa;			               AND receita.exercicio &#x3d; previsao_receita.exercicio&#xd;&#xa;			             WHERE previsao_receita.periodo &#x3d; 6&#xd;&#xa;			               AND receita.cod_entidade IN &#x28;select  unnest&#x28;regexp_split_to_array&#x28;stEntidade, E&#x27;&#x5c;,&#x27;&#x29;&#x29;&#x3a;&#x3a;int&#x29;  &#xd;&#xa;			               AND previsao_receita.exercicio &#x3d; stExercicio&#x29;&#x3a;&#x3a;VARCHAR,&#x27;.&#x27;,&#x27;,&#x27;&#x29; AS meta_arrec_bim_6&#xd;&#xa;			&#x29;       &#xd;&#xa;			, mtbiarrec_format as &#xd;&#xa;			&#x28;&#xd;&#xa;			    select &#xd;&#xa;			               coalesce&#x28;meta_arrec_bim_1&#x3a;&#x3a;varchar, &#x27;0,00&#x27;&#x29;&#xd;&#xa;			       &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;meta_arrec_bim_2&#x3a;&#x3a;varchar, &#x27;0,00&#x27;&#x29;&#xd;&#xa;			       &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;meta_arrec_bim_3&#x3a;&#x3a;varchar, &#x27;0,00&#x27;&#x29;&#xd;&#xa;			       &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;meta_arrec_bim_4&#x3a;&#x3a;varchar, &#x27;0,00&#x27;&#x29;&#xd;&#xa;			       &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;meta_arrec_bim_5&#x3a;&#x3a;varchar, &#x27;0,00&#x27;&#x29;&#xd;&#xa;			       &#x7c;&#x7c;&#x27;&#x3b;&#x27;&#x7c;&#x7c; coalesce&#x28;meta_arrec_bim_6&#x3a;&#x3a;varchar, &#x27;0,00&#x27;&#x29;&#xd;&#xa;			       as registro_mtbiarrec&#xd;&#xa;			    from mtbiarrec&#xd;&#xa;			&#x29;&#xd;&#xa;			insert into tmp_table &#x28;arquivo,content&#x29; &#xd;&#xa;			select  &#x27;MTBIARREC.csv&#x27;, json_agg&#x28;     regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;regexp_replace&#x28;  registro_mtbiarrec  ,&#x27;&#x5c;r&#x7c;&#x5c;n&#x7c;&#x5c;f&#x7c;&#x5c;t&#x27;,&#x27; &#x27;, &#x27;g&#x27;&#x29;,&#x27;&#x5e;&#x3b;&#x27;,&#x27; &#x3b;&#x27;&#x29;,&#x27;&#x3b;&#x3b;&#x27;,&#x2