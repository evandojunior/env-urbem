     , saldo_inicial AS saldo_inicial_rsp&#xd;&#xa;	                     , natureza_saldo_inicial as natureza_saldo_inicial_rsp&#xd;&#xa;	                     , total_debitos  AS total_debitos_rsp&#xd;&#xa;	                     , total_creditos  AS total_creditos_rsp&#xd;&#xa;	                     , saldo_final AS saldo_final_rsp&#xd;&#xa;	                     , natureza_saldo_final as natureza_saldo_final_rsp&#xd;&#xa;	                     , cod_unidade_sub&#xd;&#xa;	                     , cod_unidade_sub_orig&#xd;&#xa;	                from bi.balancete_arq&#xd;&#xa;	                   where exercicio &#x3d; stExercicio  															&#xd;&#xa;	                     and mes &#x3d; stMes&#x3a;&#x3a;int      																	&#xd;&#xa;	                     and cod_entidade in &#x28;select  unnest&#x28;regexp_split_to_array&#x28;stEntidade, E&#x27;&#x5c;,&#x27;&#x29;&#x29;&#x3a;&#x3a;int&#x29;    &#xd;&#xa;	                     and tipo_registro &#x3d; &#x27;14&#x27;&#xd;&#xa;	                order by  conta_contabil&#xd;&#xa;			&#x29;,&#xd;&#xa;			  bal15 as &#xd;&#xa;			&#x28;&#xd;&#xa;					select&#xd;&#xa;	                       tipo_registro&#xd;&#xa;	                     , conta_contabil&#xd;&#xa;	                     , atributo_sf&#xd;&#xa;	                     , saldo_inicial as saldo_inicial_sf&#xd;&#xa;	                     , natureza_saldo_inicial as natureza_saldo_inicial_sf&#xd;&#xa;	                     , total_debitos as total_debitos_sf&#xd;&#xa;	                     , total_creditos as total_creditos_sf&#xd;&#xa;	                     , saldo_final as saldo_final_sf&#xd;&#xa;	                     , natureza_saldo_final as natureza_saldo_final_sf&#xd;&#xa;	                from bi.balancete_arq&#xd;&#xa;	               where exercicio &#x3d; stExercicio  																&#xd;&#xa;	                     and mes &#x3d; stMes&#x3a;&#x3a;int      																	&#xd;&#xa;	                     and cod_entidade in &#x28;select  unnest&#x28;regexp_split_to_array&#x28;stEntidade, E&#x27;&#x5c;,&#x27;&#x29;&#x29;&#x3a;&#x3a;int&#x29;    &#xd;&#xa;	                 and tipo_registro &#x3d; &#x27;15&#x27;&#xd;&#xa;	            order by conta_contabil&#xd;&#xa;			&#x29;,&#xd;&#xa;			  bal16 as &#xd;&#xa;			&#x28;&#xd;&#xa;					select&#xd;&#xa;	                       tipo_registro&#xd;&#xa;	                     , conta_contabil&#xd;&#xa;	                     , cod_fonte_recursos&#xd;&#xa;	                     , atributo_sf&#xd;&#xa;	                     , saldo_inicial   AS saldo_inicial_fonte_sf&#xd;&#xa;	                     , natureza_saldo_inicial as natureza_saldo_inicial_fonte_sf&#xd;&#xa;	                     , total_debitos AS total_debitos_fonte_sf&#xd;&#xa;	                     , total_creditos  AS total_creditos_fonte_sf&#xd;&#xa;	                     , saldo_final AS saldo_final_fonte_sf&#xd;&#xa;	                     , natureza_saldo_final as natureza_saldo_final_fonte_sf&#xd;&#xa;	                from bi.balancete_arq&#xd;&#xa;	               where exercicio &#x3d; stExercicio   																&#xd;&#xa;	                     and mes &#x3d; stMes&#x3a;&#x3a;int      																	&#xd;&#xa;	                     and cod_entidade in &#x28;select  unnest&#x28;regexp_split_to_array&#x28;stEntidade, E&#x27;&#x5c;,&#x27;&#x29;&#x29;&#x3a;&#x3a;int&#x29;    &#xd;&#xa;	                 and tipo_registro &#x3d; &#x27;16&#x27;&#xd;&#xa;	            ORDER BY conta_contabil&#xd;&#xa;			&#x29;,&#xd;&#xa;  		      bal17 as &#xd;&#xa;			&#x28;&#xd;&#xa;					select&#xd;&#xa;	                       tipo_registro&#xd;&#xa;	                     , conta_contabil&#xd;&#xa;	                     , atributo_sf&#xd;&#xa;	                     , cod_ctb&#xd;&#xa;	                     , cod_fonte_recursos&#xd;&#xa;	                     , saldo_inicial  AS saldo_inicial_ctb&#xd;&#xa;	                     , natureza_saldo_inicial as natureza_saldo_inicial_ctb&#xd;&#xa;	                     , total_debitos  AS total_debitos_ctb&#xd;&#xa;	  